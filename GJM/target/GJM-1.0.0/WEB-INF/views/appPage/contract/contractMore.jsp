<%@ page contentType="text/html; charset=UTF-8" pageEncoding="UTF-8" %>
<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta name="viewport" content="width=device-width,initial-scale=1,minimum-scale=1,maximum-scale=1,user-scalable=no">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black-translucent">
    <meta name="format-detection" content="telephone=no">
    <meta name="format-detection" content="email=no">
    <title>更多操作</title>
    <link href="/resources/Plug-in/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <link href="/resources/mui/mui-scroll.css" rel="stylesheet" type="text/css">
    <link href="/resources/css/app/common.css" rel="stylesheet" type="text/css">
    <link href="/resources/css/app/contractMore.css" rel="stylesheet" type="text/css">

    <script src="/resources/js/jquery-2.0.0.min.js"></script>
    <script src="/resources/js/common/common.js"></script>
    <script src="/resources/js/appPage/contractMore.js?v=<%=System.currentTimeMillis()%>"></script>
</head>
<body>
<main>
    <div class="content">
        <div class="content-main">
            <button class="content-btn" name="conState" style="padding-left: 0px;">
                <div class="content-btn-box" style="border:none;"></div>
            </button>
        </div>
    </div>
    <div class="content">
        <div class="content-main">
            <button class="content-btn" name="conRenew">
                <div class="content-btn-box" style="border-top: none;">
                    <div class="content-btn-box-icon"><img src="/resources/image/appPage/contract/con_more_renew.png"></div>
                    <div class="content-btn-box-title">合同续约</div>
                    <div class="content-btn-box-desc"></div>
                    <div class="content-btn-box-right"><i class="fa-angle-right"></i></div>
                </div>
            </button>
            <button class="content-btn" name="conChange">
                <div class="content-btn-box">
                    <div class="content-btn-box-icon"><img src="/resources/image/appPage/contract/con_more_renew.png"></div>
                    <div class="content-btn-box-title">合同改签</div>
                    <div class="content-btn-box-desc"></div>
                    <div class="content-btn-box-right"><i class="fa-angle-right"></i></div>
                </div>
            </button>
            <button class="content-btn" name="conCancel" data-mode="add">
                <div class="content-btn-box">
                    <div class="content-btn-box-icon"><img src="/resources/image/appPage/contract/con_more_cancel.png"></div>
                    <div class="content-btn-box-title">解约订单</div>
                    <div class="content-btn-box-desc"></div>
                    <div class="content-btn-box-right"><i class="fa-angle-right"></i></div>
                </div>
            </button>
            <button class="content-btn" name="conForRent" data-mode="add">
                <div class="content-btn-box">
                    <div class="content-btn-box-icon"><img src="/resources/image/appPage/contract/con_more_forRent.png"></div>
                    <div class="content-btn-box-title">招租订单</div>
                    <div class="content-btn-box-desc"></div>
                    <div class="content-btn-box-right"><i class="fa-angle-right"></i></div>
                </div>
            </button>
            <button class="content-btn" name="conMaintain" disabled>
                <div class="content-btn-box">
                    <div class="content-btn-box-icon"><img src="/resources/image/appPage/contract/con_more_maintain.png"></div>
                    <div class="content-btn-box-title">合同维护</div>
                    <div class="content-btn-box-desc"></div>
                    <div class="content-btn-box-right"><i class="fa-angle-right"></i></div>
                </div>
            </button>
            <button class="content-btn" name="conAddItem" disabled>
                <div class="content-btn-box">
                    <div class="content-btn-box-icon"><img src="/resources/image/appPage/contract/con_more_addItem.png"></div>
                    <div class="content-btn-box-title">物品添置</div>
                    <div class="content-btn-box-desc"></div>
                    <div class="content-btn-box-right"><i class="fa-angle-right"></i></div>
                </div>
            </button>
        </div>
    </div>
    <div class="content" style="display: none;">
        <div class="content-main">
            <button class="content-btn" name="conForRentList" style="padding-left: 0px;">
                <div class="content-btn-box" style="border:none;padding-left: 16px;">
                    <div class="content-btn-box-icon"><img src="/resources/image/appPage/contract/con_more_order.png"></div>
                    <div class="content-btn-box-title">结算订单</div>
                    <div class="content-btn-box-right"><i class="fa-angle-right"></i></div>
                </div>
                <%-- <div class="content-text-list" style="border-top: 1px solid #ddd;padding-left: 16px;display: none;">
                    <button class="content-btn-box" style="line-height: 34px;border-top: none;">
                        <div class="content-btn-box-title" style="flex-direction: column;">
                            <div class="next">转租招租</div>
                            <div style="font-size: 12px;color: #666;">蒋庆涛 - 15123384649</div>
                        </div>
                        <div class="content-btn-box-text" style="padding-right: 10px;">
                            <div class="error">200元</div>
                            <div style="font-size: 12px;color: #666;">蒋庆涛</div>
                        </div>
                    </button>
                </div> --%>
            </button>
        </div>
    </div>
</main>
</body>
</html>