.publish-box .sub-content {
    padding: 10px 0;
}
.publish-box dl.info-box > dt.item {
    width: 80px;
}
.publish-box dl.info-box > dd.item {
    color: #059679;
    min-width: 300px;
    max-width: 600px;
}
.publish-box-content .common-checkbox {
    position: absolute;
    width: 100%;
    height: 100%;
    padding: 0;
    margin: 0;
    top: 0;
    left: 0;
}
.publish-box-content .common-checkbox:BEFORE {
    top: 6px;
    left: 6px;
    border-radius: 10px;
    background-color: #fff;
}
.publish-box-content .common-checkbox-checked {
    /*     background: rgba(216, 214, 214, 0.37); */
}
.publish-house-label {
    position: relative;
    border: 1px dashed #1ABC9C;
    background: #1ABC9C;
    color: #fff;
    margin: 4px 0;
    padding: 0 10px;
    line-height: 24px;
    font-size: 12px;
    display: block;
    float: left;
    margin-right: 12px;
    border-radius: 2px;
}
.housePrice {
    padding: 10px;
}
.housePrice input {
    width: 60px;
    height: 30px;
    line-height: 30px;
    margin-right: 3px;
    border-radius: 3px;
}
#model-box {
    z-index: 9999;
}
.updateContent {
    background: #ededed;
    width: 100%;
    overflow: hidden;
    height: auto;
}
.updateContent dl {
    width: 50%;
    float: left;
}
.updateContent dl dt {
    float: left;
    height: 50px;
    line-height: 50px;
    width: 90px;
    text-align: right;
    padding-right: 15px;
}
.updateContent dl dd {
    float: left;
    height: 50px;
    line-height: 50px;
}
.updateContent dl dd input {
    height: 32px;
    line-height: 32px;
    background: #fdf7f7;
    border-radius: 3px;
}
.updateContent button {
    padding: 0 15px;
    height: 30px;
    line-height: 30px;
    background: #3498DB;
    color: #FFF;
    border-radius: 3px;
    margin-right: 24px;
    margin-bottom: 10px;
    float: right;
}
.checkbox {
    min-width: 43px;
    padding: 1px 10px;
    border-radius: 5px;
    background-color: #F2F2F2;
    float: left;
    height: 22px;
    line-height: 22px;
    margin-top: 1%;
    margin-right: 3%;
    margin-bottom: 10%;
    text-align: center;
    cursor: pointer;
}
.checkbox.click {
    background-color: #FF6666;
    color: #FFF;
}
.checkbox i {
    font-style: normal;
    font-size: 13px;
}
.tangram-suggestion-main {
    z-index: 1060;
}