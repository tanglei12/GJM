package com.gjp.model;

import java.util.Date;

/**
 * @author 陈智颖
 *
 * @version 创建时间：2016年4月11日 下午5:40:17 
 */
public class PropertyLivingPayment {
	
	//物业水电气编码
	private Integer lp_id;
	//类型名（水、电、气）
	private String lp_type;
	//缴费周期
	private String lp_cycle;
	//价格
	private Double lp_money;
	//特殊分段
	private String lp_duan;
	//公司
	private String lp_company;
	//联系人
	private String lp_name;
	//电话
	private String lp_phone;
	//是否阶梯计费
	private Integer lp_bool;
	//时间
	private Date lp_date;
	//内部人员编码
	private Integer em_id;
	//是否通水、电、气
	private Integer lp_bools;
	//备注
	private String lp_beizhu;
	//物业信息编码
	private Integer propertyInfo_Id;
	//物业水电气总数
	private Integer size;
	
	public PropertyLivingPayment(){}

	public Integer getLp_id() {
		return lp_id;
	}

	public void setLp_id(Integer lp_id) {
		this.lp_id = lp_id;
	}

	public String getLp_type() {
		return lp_type;
	}

	public void setLp_type(String lp_type) {
		this.lp_type = lp_type;
	}

	public String getLp_cycle() {
		return lp_cycle;
	}

	public void setLp_cycle(String lp_cycle) {
		this.lp_cycle = lp_cycle;
	}

	public Double getLp_money() {
		return lp_money;
	}

	public void setLp_money(Double lp_money) {
		this.lp_money = lp_money;
	}

	public String getLp_duan() {
		return lp_duan;
	}

	public void setLp_duan(String lp_duan) {
		this.lp_duan = lp_duan;
	}

	public String getLp_company() {
		return lp_company;
	}

	public void setLp_company(String lp_company) {
		this.lp_company = lp_company;
	}

	public String getLp_name() {
		return lp_name;
	}

	public void setLp_name(String lp_name) {
		this.lp_name = lp_name;
	}

	public String getLp_phone() {
		return lp_phone;
	}

	public void setLp_phone(String lp_phone) {
		this.lp_phone = lp_phone;
	}

	public Integer getLp_bool() {
		return lp_bool;
	}

	public void setLp_bool(Integer lp_bool) {
		this.lp_bool = lp_bool;
	}

	public Date getLp_date() {
		return lp_date;
	}

	public void setLp_date(Date lp_date) {
		this.lp_date = lp_date;
	}

	public Integer getEm_id() {
		return em_id;
	}

	public void setEm_id(Integer em_id) {
		this.em_id = em_id;
	}

	public String getLp_beizhu() {
		return lp_beizhu;
	}

	public void setLp_beizhu(String lp_beizhu) {
		this.lp_beizhu = lp_beizhu;
	}

	public Integer getPropertyInfo_Id() {
		return propertyInfo_Id;
	}

	public void setPropertyInfo_Id(Integer propertyInfo_Id) {
		this.propertyInfo_Id = propertyInfo_Id;
	}

	public Integer getLp_bools() {
		return lp_bools;
	}

	public void setLp_bools(Integer lp_bools) {
		this.lp_bools = lp_bools;
	}

	public Integer getSize() {
		return size;
	}

	public void setSize(Integer size) {
		this.size = size;
	}
}
