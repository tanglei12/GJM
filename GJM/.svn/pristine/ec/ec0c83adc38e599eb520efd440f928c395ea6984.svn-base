package com.gjp.model;

import java.util.Date;

/**
 * 任务消息
 * 
 * @author 陈智颖
 *
 * @version 创建时间：2016年9月5日 下午5:37:29 
 */
public class UserCenterTaskMessage {

	// 任务消息编码
	private Integer tm_id;
	// 任务消息内容
	private String tm_text;
	// 任务消息类型 0：个人 1：部门 2：公司
	private Integer tm_type;
	// 事件紧急程度 0：紧急 1：重要 2：计划
	private Integer tm_state;
	// 任务消息开始时间
	private Date tm_startTime;
	// 任务消息开始星期
	private String tm_startWeek;
	// 任务消息结束时间
	private Date tm_endTime;
	// 任务消息结束星期
	private String tm_endWeek;
	// 跳转任务页面URL
	private String tm_href;
	// 处理结果
	private Integer tm_result;
	// 内部人员编码
	private Integer em_id;
	// 备注
	private String tm_beizhu;
	// 任务消息父级编码
	private Integer tm_pid;
	// 安排人任务
	private String userTask;
	// 用户名
	private String em_name;
	// 安排人
	private String mainUser;
	// 0：手动 1：系统
	private Integer tm_tmType;
	// 调用地址
	private String tm_http;
	// 时间
	private String start;
	private Integer pageNo;
	private int size;
	
	public UserCenterTaskMessage(){}

	public Integer getTm_id() {
		return tm_id;
	}

	public void setTm_id(Integer tm_id) {
		this.tm_id = tm_id;
	}

	public String getTm_text() {
		return tm_text;
	}

	public void setTm_text(String tm_text) {
		this.tm_text = tm_text;
	}

	public Integer getTm_type() {
		return tm_type;
	}

	public void setTm_type(Integer tm_type) {
		this.tm_type = tm_type;
	}

	public Integer getTm_state() {
		return tm_state;
	}

	public void setTm_state(Integer tm_state) {
		this.tm_state = tm_state;
	}

	public Date getTm_startTime() {
		return tm_startTime;
	}

	public void setTm_startTime(Date tm_startTime) {
		this.tm_startTime = tm_startTime;
	}

	public String getTm_startWeek() {
		return tm_startWeek;
	}

	public void setTm_startWeek(String tm_startWeek) {
		this.tm_startWeek = tm_startWeek;
	}

	public Date getTm_endTime() {
		return tm_endTime;
	}

	public void setTm_endTime(Date tm_endTime) {
		this.tm_endTime = tm_endTime;
	}

	public String getTm_endWeek() {
		return tm_endWeek;
	}

	public void setTm_endWeek(String tm_endWeek) {
		this.tm_endWeek = tm_endWeek;
	}

	public String getTm_href() {
		return tm_href;
	}

	public void setTm_href(String tm_href) {
		this.tm_href = tm_href;
	}

	public Integer getTm_result() {
		return tm_result;
	}

	public void setTm_result(Integer tm_result) {
		this.tm_result = tm_result;
	}

	public Integer getEm_id() {
		return em_id;
	}

	public void setEm_id(Integer em_id) {
		this.em_id = em_id;
	}

	public String getTm_beizhu() {
		return tm_beizhu;
	}

	public void setTm_beizhu(String tm_beizhu) {
		this.tm_beizhu = tm_beizhu;
	}

	public Integer getTm_pid() {
		return tm_pid;
	}

	public void setTm_pid(Integer tm_pid) {
		this.tm_pid = tm_pid;
	}

	public String getUserTask() {
		return userTask;
	}

	public void setUserTask(String userTask) {
		this.userTask = userTask;
	}

	public String getEm_name() {
		return em_name;
	}

	public void setEm_name(String em_name) {
		this.em_name = em_name;
	}

	public String getMainUser() {
		return mainUser;
	}

	public void setMainUser(String mainUser) {
		this.mainUser = mainUser;
	}

	public Integer getTm_tmType() {
		return tm_tmType;
	}

	public void setTm_tmType(Integer tm_tmType) {
		this.tm_tmType = tm_tmType;
	}

	public String getTm_http() {
		return tm_http;
	}

	public void setTm_http(String tm_http) {
		this.tm_http = tm_http;
	}

	public String getStart() {
		return start;
	}

	public Integer getPageNo() {
		return pageNo;
	}

	public void setPageNo(Integer pageNo) {
		this.pageNo = pageNo;
	}

	public void setStart(String start) {
		this.start = start;
	}

	public int getSize() {
		return size;
	}

	public void setSize(int size) {
		this.size = size;
	}
	
}
