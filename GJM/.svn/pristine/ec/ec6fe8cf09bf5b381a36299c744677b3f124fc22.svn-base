package com.gjp.model;

/**
 * 地图搜房条件筛选
 *
 * @author 陈智颖
 * @create 2017-08-23 10:59 AM
 **/
public class HouseMapWhere {

    // 房屋类型编码
    private Integer his_id;
    // 房屋类型
    private String his_name;
    // 户型
    private String hi_houseType;
    // 租金
    private String hi_money;
    // 租金排序
    private String hi_moneyAscDesc;
    // 房源
    private String hi_function;
    // 开始价格
    private Double moneyStart;
    // 结束价格
    private Double moneyEnd;
    // 物业编号
    private Integer upn_id;
    // 物业超父级编号
    private Integer upn_sid;
    // 用户编码
    private Integer user_id;

    public Integer getHis_id() {
        return his_id;
    }

    public void setHis_id(Integer his_id) {
        this.his_id = his_id;
    }

    public String getHis_name() {
        return his_name;
    }

    public void setHis_name(String his_name) {
        this.his_name = his_name;
    }

    public String getHi_money() {
        return hi_money;
    }

    public void setHi_money(String hi_money) {
        this.hi_money = hi_money;
    }

    public String getHi_moneyAscDesc() {
        return hi_moneyAscDesc;
    }

    public void setHi_moneyAscDesc(String hi_moneyAscDesc) {
        this.hi_moneyAscDesc = hi_moneyAscDesc;
    }

    public String getHi_function() {
        return hi_function;
    }

    public void setHi_function(String hi_function) {
        this.hi_function = hi_function;
    }

    public String getHi_houseType() {
        return hi_houseType;
    }

    public void setHi_houseType(String hi_houseType) {
        this.hi_houseType = hi_houseType;
    }

    public Double getMoneyStart() {
        return moneyStart;
    }

    public void setMoneyStart(Double moneyStart) {
        this.moneyStart = moneyStart;
    }

    public Double getMoneyEnd() {
        return moneyEnd;
    }

    public void setMoneyEnd(Double moneyEnd) {
        this.moneyEnd = moneyEnd;
    }

    public Integer getUpn_id() {
        return upn_id;
    }

    public void setUpn_id(Integer upn_id) {
        this.upn_id = upn_id;
    }

    public Integer getUpn_sid() {
        return upn_sid;
    }

    public void setUpn_sid(Integer upn_sid) {
        this.upn_sid = upn_sid;
    }

    public Integer getUser_id() {
        return user_id;
    }

    public void setUser_id(Integer user_id) {
        this.user_id = user_id;
    }
}
