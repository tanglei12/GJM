package com.gjp.model;

import java.util.Date;

/**
 * 手机验证码
 * 
 * @author 王孝元
 * @version 创建时间：2017年3月20日 上午10:46:59
 * 
 */
public class MobileCode {

	// 编号
	private Integer mc_id;
	// 手机
	private String mc_phone;
	// 验证码
	private String mc_code;
	// 类型
	private String mc_type;
	// 创建时间
	private Date mc_createTime;
	// 失效时间
	private Date mc_destroyTime;
	// 是否已使用（验证码只能使用一次）
	private Integer mc_isUse;

	public String getMc_code() {
		return mc_code;
	}

	public void setMc_code(String mc_code) {
		this.mc_code = mc_code;
	}

	public Date getMc_createTime() {
		return mc_createTime;
	}

	public void setMc_createTime(Date mc_createTime) {
		this.mc_createTime = mc_createTime;
	}

	public Integer getMc_id() {
		return mc_id;
	}

	public void setMc_id(Integer mc_id) {
		this.mc_id = mc_id;
	}

	public String getMc_phone() {
		return mc_phone;
	}

	public void setMc_phone(String mc_phone) {
		this.mc_phone = mc_phone;
	}

	public String getMc_type() {
		return mc_type;
	}

	public void setMc_type(String mc_type) {
		this.mc_type = mc_type;
	}

	public Date getMc_destroyTime() {
		return mc_destroyTime;
	}

	public void setMc_destroyTime(Date mc_destroyTime) {
		this.mc_destroyTime = mc_destroyTime;
	}

	public Integer getMc_isUse() {
		return mc_isUse;
	}

	public void setMc_isUse(Integer mc_isUse) {
		this.mc_isUse = mc_isUse;
	}

}
