/**5*/
.form-hint {
    position: relative;
    width: 100%;
    line-height: 30px;
    margin-bottom: 10px;
    border: 1px solid #ff9a90;
    padding: 0 10px;
    background: #ffdcd9;
    border-radius: 2px;
}
.form-hint:AFTER {
    content: "\f00d";
    font: normal normal normal 14px/1 FontAwesome;
    position: absolute;
    top: 7px;
    right: 8px;
}
.form {
    font-size: 14px;
    width: 100%;
}
