@CHARSET "UTF-8";
.type_on_off {
    position: relative;
    height: 28px;
    padding: 0 4px;
    border: 1px solid #ddd;
    border-radius: 4px;
    font-size: 12px;
}
table.option-subtable {
    font-size: 12px;
    background: #fff;
    box-shadow: 0 2px 4px #ddd;
}
table.option-subtable tr {
}
table.option-subtable thead tr {
    background: #e1f1fb;
}
table.option-subtable tbody tr:HOVER {
    background: #f5f8fa !important;
}
table.option-subtable tr > td {
    position: relative;
    line-height: 30px;
    border: 1px solid #ddd;
    text-align: center;
    white-space: nowrap;
}
table.option-subtable tr > td .option-subtable-select {
    position: relative;
    float: left;
    border: none;
    height: 30px;
    line-height: 30px;
    font-size: 12px;
    padding: 0 6px 0 10px;
    color: #fff;
    border-radius: 4px;
    cursor: pointer;
}
table.option-subtable tr > td .option-subtable-select:AFTER {
    content: "\f105";
    font: normal normal normal 14px/1 FontAwesome;
    display: inline-block;
    position: relative;
    top: 2px;
    width: 24px;
    margin-left: 2px;
    margin-right: -4px;
    font-size: 18px;
    text-align: center;
}
table.option-subtable tr > td.budget-bill-address {
    padding: 0 16px 0 8px;
    text-align: left;
    cursor: pointer;
    width: 140px;
    text-overflow: ellipsis;
    white-space: nowrap;
}
table.option-subtable tr > td .option-subtable-select:FOCUS {
    border-color: #66afe9;
    outline: 0;
    -webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075), 0 0 8px rgba(102, 175, 233, .6);
    box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075), 0 0 8px rgba(102, 175, 233, .6)
}
table.option-subtable tr > td .option-subtable-option {
    position: absolute;
    left: 100px;
    top: -1px;
    width: 520px;
    font-size: 12px;
    /*     padding: 6px; */
    background: #fff;
    border: 1px solid #ddd;
    box-shadow: 0 0 6px #ddd;
    border-radius: 4px;
    z-index: 10;
}
table.option-subtable tr > td .option-subtable-option .subtable-option-head {
    position: relative;
    line-height: 30px;
    color: #000;
    padding-top: 8px;
    padding-right: 8px;
    font-size: 14px;
}
table.option-subtable tr > td .option-subtable-option .subtable-option-main {
    position: relative;
    padding-top: 8px;
    padding-right: 8px;
    max-height: 168px;
    border-top: 1px solid #ddd;
}
table.option-subtable tr > td .option-subtable-option .subtable-option-head:AFTER,
table.option-subtable tr > td .option-subtable-option .subtable-option-main:AFTER {
    content: "";
    display: block;
    clear: both;
}
table.option-subtable tr > td .option-subtable-option [class^=subtable-option-main-sub] {
    position: relative;
    float: left;
    background: #fff;
    padding: 0 10px;
    text-align: center;
    line-height: 28px;
    margin: 0 0 8px 8px;
    border: 1px solid #3498db;
    color: #3498db;
    cursor: pointer;
    border-radius: 3px;
}
table.option-subtable tr > td .option-subtable-option .subtable-option-main [class^=subtable-option-main-sub] {
    border: 1px solid #1abc9c;
    color: #1abc9c;
}
table.option-subtable tr > td .option-subtable-option .subtable-option-main [class^=subtable-option-main-sub]:disabled {
    border: 1px solid #ccc;
    color: #ccc;
}
table.option-subtable tr > td .fa-info-circle {
    position: absolute;
    top: 50%;
    right: 6px;
    margin-top: -6px;
    font-size: 14px;
}
table.option-subtable tr > td .fa-info-circle:HOVER + .cycle-more-option {
    display: block;
}
table.option-subtable tr > td select.option-subtable-select > option {
    background: #fff;
    color: #666
}
table.option-subtable tr > td select.option-subtable-select > option[disabled] {
    background: #ddd;
    color: #fff
}
table.option-subtable tr > td input.budget-input {
    position: relative;
    width: 100%;
    line-height: 30px;
    text-align: center;
    background: rgba(251, 174, 174, 0.14);
    padding: 0 10px;
    font-size: 12px;
}
table.option-subtable tr > td input.budget-input[readonly] {
    cursor: pointer;
}
table.option-subtable tr > td input.budget-input[disabled] {
    background: rgba(93, 91, 91, 0.07);
}
table.option-subtable tr > td input.budget-input:FOCUS,
table.option-subtable tr > td select.budget-input:FOCUS,
table.option-subtable tr > td button.sub-option-btn:FOCUS {
    border-color: #66afe9;
    outline: 0;
    -webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075), 0 0 8px rgba(102, 175, 233, .6);
    box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075), 0 0 8px rgba(102, 175, 233, .6);
}
table.option-subtable tr.subtable-add .item-index {
    display: inline-block;
    line-height: 20px;
    width: 20px;
    border-radius: 10px;
    background: #1abc9c;
    color: #fff;
}
.char-sepa {
    position: relative;
    display: inline-block;
    margin-left: 10px;
    float: left;
}
.char-sepa-first {
    display: inline-block;
    float: left;
    font-size: 14px;
    color: #000;
}
.char-sepa-split {
    display: inline-block;
    margin-left: 10px;
    float: left;
    color: #ddd;
}
.char-sepa-text {
    display: inline-block;
    margin: 0 4px;
}
.order-hint-yq {
    position: relative;
    top: 8px;
    display: block;
    float: right;
    line-height: 20px;
    padding: 0 4px;
    background: #f16d60;
    color: #fff;
    border-radius: 4px;
}
.order-hint-yq:BEFORE {
    content: attr(data-title);
    display: inline-block;
    margin-right: 2px;
}
.option-subtable-box {
    position: relative;
    display: inline-block;
    float: left;
    margin-left: 10px;
    background: #fff;
}
.option-subtable-box:AFTER {
    content: "";
    display: block;
    clear: both;
}
.option-subtable-box .order-box-icon {
    position: relative;
    float: left;
    display: inline-block;
    height: 30px;
    line-height: 28px;
    padding: 0 8px;
    font-size: 12px;
    border-top-left-radius: 3px;
    border-bottom-left-radius: 3px;
    text-align: center;
    border: 1px solid #ddd;
    border-right: none;
    color: #34495e;
    background: #fbfbfb;
}
.option-subtable-box .order-box-title {
    position: relative;
    float: left;
    display: inline-block;
    line-height: 28px;
    padding: 0 10px;
    border-top-left-radius: 3px;
    border-bottom-left-radius: 3px;
    text-align: center;
    font-size: 12px;
    border: 1px solid #ddd;
    border-right: none;
    color: #34495e;
}
.option-subtable-box .order-box-content {
    position: relative;
    display: inline-block;
    float: left;
    text-align: left;
    padding-right: 10px;
    border: 1px solid #ddd;
    border-right: 0;
    border-left: 0;
    height: 30px;
    font-size: 14px;
}
.option-subtable-box .order-box-info {
    position: relative;
    display: inline-block;
    float: left;
    text-align: left;
    min-width: 140px;
    padding: 0 10px;
    border: 1px solid #ddd;
    border-right: 0;
    height: 30px;
    line-height: 28px;
    font-size: 12px;
    background: #fff6f5;
    cursor: pointer;
}
.option-subtable-box .order-box-suffix {
    position: relative;
    float: left;
    display: inline-block;
    height: 30px;
    line-height: 28px;
    padding: 0 10px;
    border-top-right-radius: 3px;
    border-bottom-right-radius: 3px;
    text-align: center;
    font-size: 12px;
    border: 1px solid #ddd;
    color: #34495e;
}
.option-subtable-box .order-box-model {
    position: absolute;
    bottom: 36px;
    right: 0;
    width: 281px;
    box-shadow: 0 0 4px #ccc;
    background: #fff;
    border: 1px solid #ddd;
}
.order-box-model > .box-model-item {
    position: relative;
}
.order-box-model > .box-model-item > dl {
    position: relative;
    padding: 0 10px;
    line-height: 29px;
    border-bottom: 1px solid #ddd;
}
.order-box-model > .box-model-item > dl > dt {
    display: inline-block;
    float: left;
    padding-right: 10px;
}
.order-box-model > .box-model-item > dl > dd {
    display: inline-block;
    float: left;
    padding-right: 10px;
}
.option-subtable-box input.order-box-info:FOCUS {
    border-color: #66afe9;
    outline: 0;
    -webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075), 0 0 8px rgba(102, 175, 233, .6);
    box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075), 0 0 8px rgba(102, 175, 233, .6)
}
.option-subtable-box select.order-box-info {
    min-width: inherit;
}
.option-subtable-box .order-box-btn {
    position: relative;
    display: inline-block;
    float: left;
    padding: 0 10px;
    line-height: 28px;
    background: #fbfbfb;
    font-size: 12px;
    border: 1px solid #ddd;
    border-top-right-radius: 3px;
    border-bottom-right-radius: 3px;
    cursor: pointer;
}
.option-subtable-handle {
    position: relative;
    float: left;
    line-height: 30px;
    font-size: 12px;
    padding: 0 10px;
    border-radius: 3px;
    background: #3498db;
    color: #fff;
    margin-left: 10px;
    cursor: pointer;
}
.option-subtable-handle:FOCUS {
    border-color: #66afe9;
    outline: 0;
    -webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075), 0 0 8px rgba(102, 175, 233, .6);
    box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075), 0 0 8px rgba(102, 175, 233, .6)
}
.option-subtable-handle > [class^=fa-] {
    margin-right: 6px;
    font-size: 14px;
    position: relative;
    top: 1px;
}
.option-subtable-handle[disabled] {
    background: #ddd;
}
.corner {
    position: absolute;
    top: -8px;
    right: 28px;
    width: 0;
    height: 0;
    border-bottom: 8px solid #a0a0a0;
    border-left: 8px solid transparent;
    border-right: 8px solid transparent;
    z-index: 1;
}
.corner:BEFORE {
    position: absolute;
    top: -8px;
    right: 28px;
    width: 0;
    height: 0;
    border-bottom: 8px solid #a0a0a0;
    border-left: 8px solid transparent;
    border-right: 8px solid transparent;
    z-index: 1;
}
div[class^=order-model-box] {
    position: absolute;
    top: 0;
    background: #fff;
    box-shadow: 0 0 10px rgba(0, 0, 0, 0.2);
    margin-bottom: 10px;
    z-index: 10;
}
div[class^=order-model-box] .fa-reorder {
    display: inline-block;
    line-height: 20px;
    padding: 0 12px;
    font-size: 14px;
    background: none;
    border-radius: 10px;
    color: #3498db;
    cursor: pointer;
}
div[class^=order-model-box] .fa-reorder:HOVER {
    background: #3498db;
    color: #fff;
}
.td-remarks {
    position: relative;
    display: inline-block;
    float: left;
    padding: 0 10px;
    text-align: left;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    width: 400px;
}
.fa-outdent {
    position: relative;
    float: right;
    display: inline-block;
    font-size: 14px;
    top: 12px;
    color: #3498db;
}
/***/
.budget-order-box {
    padding: 10px;
}
.budget-order-box table {
    font-size: 12px;
    line-height: 30px;
}
.budget-order-box table tr > th {
    font-weight: normal;
    color: #000;
    background: #e1f1fb;
}
.budget-order-box table tbody tr {
    border-bottom: 1px solid #ddd;
    cursor: pointer;
}
.budget-order-box table tr > td {
    text-align: center;
}
.order-box-model {
    position: relative;
    padding-top: 12px;
    text-align: center;
    font-size: 12px;
}
.order-box-model .budget-model-addIcon {
    position: relative;
    left: -3px;
    line-height: 30px;
    width: 30px;
    height: 30px;
    border-radius: 15px;
    background: #3498db;
    margin: 0 auto;
    cursor: pointer;
    color: #FFF;
    z-index: 2;
}
.order-box-model .budget-model-add {
    position: absolute;
    bottom: 60%;
    left: 50%;
    margin-left: -130px;
    display: inline-block;
    padding: 12px;
    width: 260px;
    font-size: 14px;
    border-radius: 3px;
    background: #fff;
    border: 1px solid #ddd;
    box-shadow: 0 0 10px #ddd;
    z-index: 1;
}
.order-box-model .budget-model-add > dl {
    position: relative;
}
.order-box-model .budget-model-add > dl > dt {
    position: relative;
    display: block;
    text-align: left;
    padding: 0 8px;
    font-size: 12px;
    line-height: 28px;
}
.order-box-model .budget-model-add > dl > dd {
    position: relative;
    display: block;
    text-align: left;
    line-height: 28px;
    border: 1px solid #ddd;
    border-bottom: none;
}
.order-box-model .budget-model-add > dl > dd:FIRST-CHILD {
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
}
.order-box-model .budget-model-add > dl > dd:LAST-CHILD {
    border-bottom-left-radius: 4px;
    border-bottom-right-radius: 4px;
    border-bottom: 1px solid #ddd;
    border-right: none;
    border-left: none;
}
.order-box-model .budget-model-add > dl > dd > input,
.order-box-model .budget-model-add > dl > dd > select,
.order-box-model .budget-model-add > dl > dd > textarea {
    position: relative;
    display: block;
    padding: 6px 10px;
    line-height: 26px;
    border: none;
    width: 100%;
    border-radius: 2px;
    font-size: 12px;
    resize: none;
}
.order-box-model .budget-model-add > dl > dd > textarea {
    line-height: 20px;
}
.order-box-model .budget-model-add > dl > dd > select {
    height: 38px;
    padding: 6px 7px;
}
.order-box-model .budget-model-add > dl > dd > input[required],
.order-box-model .budget-model-add > dl > dd > select[required],
.order-box-model .budget-model-add > dl > dd > textarea[required] {
    background: #fff6f5;
}
.order-box-model .budget-model-add > dl > dd > input[readonly],
.order-box-model .budget-model-add > dl > dd > textarea[readonly] {
    background: #f5f0f0;
    cursor: pointer;
}
.order-box-model .budget-model-add > dl > dd > button {
    position: relative;
    line-height: 34px;
    width: 100%;
    padding: 0 10px;
    border-bottom-left-radius: 4px;
    border-bottom-right-radius: 4px;
    font-size: 12px;
    color: #fff;
    background: #1ABC9C;
    cursor: pointer;
}
.order-box-model .page-option {
    position: relative;
    display: block;
    width: 32px;
    line-height: 28px;
    float: left;
    border: 1px solid #ddd;
    border-left: none;
    color: #3399d5;
    background: #fff;
    -moz-user-select: none;
    -webkit-user-select: none;
}
.order-box-model .fa-angle-left {
    border-top-left-radius: 4px;
    border-bottom-left-radius: 4px;
    font-size: 18px;
    border-left: 1px solid #ddd;
    cursor: pointer;
}
.order-box-model .fa-angle-right {
    border-top-right-radius: 4px;
    border-bottom-right-radius: 4px;
    font-size: 18px;
    cursor: pointer;
}
.common-budget-checkbox > input {
    display: none;
}
.common-budget-checkbox {
    position: relative;
    height: 22px;
    line-height: 22px;
    padding-left: 24px;
    display: inline-block;
    cursor: pointer;
    -moz-user-select: none;
    -webkit-user-select: none;
}
.common-budget-checkbox:BEFORE {
    content: "";
    display: block;
    position: absolute;
    top: 1px;
    left: 1px;
    width: 20px;
    height: 20px;
    background: url(/resources/image/checkbox.png) no-repeat 0 -90px;
}
.sub-option-btn {
    position: relative;
    display: inline-block;
    line-height: 24px;
    padding: 0 6px;
    border-radius: 3px;
    font-size: 12px;
    color: #fff;
    background: #1ABC9C;
    cursor: pointer;
}
/**3*/
.more-option-box {
    position: absolute;
    right: 0;
    display: inline-block;
    text-align: center;
    float: right;
    height: 100%;
}
.more-option-box .fa-angle-double-right {
    display: inline-block;
    line-height: 30px;
    width: 16px;
    font-size: 18px;
    color: #5a9edb;
    background: none;
    cursor: pointer;
}
.more-option-model {
    display: none;
    position: absolute;
    left: 105%;
    top: 0;
    background: #fff;
    box-shadow: 0 0 10px rgba(0, 0, 0, 0.27);
    padding: 8px 0 0 8px;
    border-radius: 2px;
    z-index: 2;
}
.more-option-model:AFTER {
    content: "";
    display: block;
    clear: both;
}
.more-option-model .more-option-model-icon {
    position: absolute;
    top: 9px;
    left: -6px;
    width: 0;
    height: 0;
    border-right: 6px solid #eaeaea;
    border-top: 6px solid transparent;
    border-bottom: 6px solid transparent;
    z-index: 100;
}
.more-option-model .more-option-model-icon:BEFORE {
    content: "";
    position: absolute;
    top: -6px;
    left: 1px;
    width: 0;
    height: 0;
    border-right: 6px solid #fff;
    border-top: 6px solid transparent;
    border-bottom: 6px solid transparent;
}
.more-option-model button.option-model-btn {
    display: block;
    padding: 4px 6px;
    border-radius: 3px;
    margin-bottom: 8px;
    font-size: 12px;
    cursor: pointer;
}
/* .more-option-model button.option-model-btn:LAST-CHILD{margin-bottom: 0px;} */
/* .more-option-model .more-option-model-content{position: relative;} */
/* .more-option-model .more-option-model-content:AFTER{content: "";display: block;clear: both;} */
.more-option-model .model-content-body {
    position: relative;
    float: left;
}
.more-option-model button.model-body-item {
    display: block;
    float: left;
    text-align: center;
    padding: 4px 0;
    border-radius: 3px;
    margin-bottom: 8px;
    margin-right: 8px;
    width: 60px;
    font-size: 12px;
    border: 1px solid #1abc9c;
    color: #1abc9c;
    background: none;
    cursor: pointer;
}
