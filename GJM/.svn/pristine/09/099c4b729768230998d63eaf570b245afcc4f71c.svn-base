/**###*/
.form-list-remarks {
    position: relative;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    text-align: left;
    width: 300px;
}
.form-list-remarks:HOVER {
    white-space: inherit;
}
.form-model-box {
    position: absolute;
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, 0.13);
    z-index: 99;
}
.form-model-box .form-model-main {
    position: absolute;
    top: 8%;
    left: 50%;
    margin-left: -25%;
    border-radius: 4px;
    width: 50%;
    height: auto;
    border: 1px solid #ddd;
    box-shadow: 0 0 10px rgba(0, 0, 0, 0.09);
    background: #fff;
}
.form-model-box .form-model-main .form-model-head {
    position: relative;
    line-height: 36px;
    border-bottom: 1px solid #ddd;
    margin-bottom: 10px;
    padding-left: 10px;
    color: #000;
}
.form-model-box .form-model-main .form-model-head > .fa-close {
    position: absolute;
    top: 4px;
    right: 4px;
    height: 28px;
    width: 28px;
    font-size: 16px;
    border-radius: 3px;
    background: #fff;
    color: #E74C3C;
    cursor: pointer;
}
.form-model-box .form-model-main .form-model-head > .fa-close:HOVER {
    background: #E74C3C;
    color: #fff;
}
