*{padding: 0;margin: 0;-webkit-box-sizing: border-box;-moz-box-sizing: border-box;box-sizing: border-box;}
:after,:before {-webkit-box-sizing: border-box;-moz-box-sizing: border-box;box-sizing: border-box;}
body {font: 12px/150% '微软雅黑',Arial, Verdana, "\5b8b\4f53";min-width: 980px;color: #666;}

table{width: 100%;border-collapse:collapse;border-spacing: 0;}
hr{clear: both;padding: 0;margin: 0}
ul,li{list-style:none;}
hr{border: none;}
a{text-decoration: none !important;}
a:HOVER{text-decoration: none !important;}
/** AFTER去浮动*/
li:AFTER,
ul:AFTER,
dl:AFTER,
.clear:AFTER,
.sub-title:AFTER{content: ""; display: block; clear: both;}

.titleFont{
	height: 60px;
	line-height: 60px;
	text-align: center;
	font-size: 25px;
	color:#000;
	position: relative;
}
.houseCode{
	margin-left: 10px;
	float: left;
}
.houseCode dl dt{
	float: left;
	color: #000;
}
.houseCode dl dd{
	float: left;
	margin-left: 5px;
}
.houseDate{
	float: right;
	margin-right: 10px;
}
.houseDate dl dt{
	float: left;
	color: #000;
}
.houseDate dl dd{
	float: left;
	margin-left: 5px;
}
table{
	border-collapse: collapse;
	border: 1px solid #e3e4e6;
}
table td{
    border: 1px solid #e3e4e6;
    height: 25px;
   	line-height: 25px;
   	font-size: 13px;
}
table td.taTitle{
   font-size: 13px;
   height: 25px;
   line-height: 25px;
   width: 100px;
   text-align: center;
   color:#000;
}
table td.taTitleT{
   font-size: 15px;
   height: 35px;
   line-height: 35px;
   text-align: center;
   color:#000;
}
.moreMessage{
    position: absolute;
    right: 395px;
    top: 130px;
    height: 35px;
    line-height: 35px;
    color: #29e;
    font-size: 13px;
}
.contentDiv{
	max-width: 1090px;
    border-radius: 2px;
    box-shadow: 0px 0px 6px rgba(0, 0, 0, 0.2);
    padding:10px;
    margin-left: 10px;
    margin-top: 5px;
    margin-bottom: 5px;
    position: relative;
}
.moreList{
/* 	border: 1px dashed #29e; */
display: none;
}
.moreList table{
	border: 0;
}
.borderMore{
	position:absolute;
	top: 140px;
	left: 0;
	width: 100%;
	height: 100%;
	border: 1px dashed #29e;
	margin: 0 10px;
}