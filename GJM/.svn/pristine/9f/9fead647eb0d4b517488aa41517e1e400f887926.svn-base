}
.repay-tag {
    display: block;
    position: absolute;
    width: 34px;
    height: 32px;
    top: 0;
    left: 0;
    background-size: 100% 100%;
    background: url("/resources/image/repay-icon.png") no-repeat;
}
.paybutton {
    width: 100%;
    padding: 5px 0;
    min-height: 43px;
    position: relative;
}
.paybutton button {
    position: relative;
    float: left;
    border: none;
    height: 32px;
    line-height: 32px;
    font-size: 12px;
    padding: 0 15px;
    background: #1ABC9C;
    color: #fff;
    border-radius: 4px;
    cursor: pointer;
}
.paybutton .insert {
    margin-left: 12px;
    position: relative;
    float: left;
    border: none;
    height: 32px;
    line-height: 32px;
    font-size: 12px;
    padding: 0 6px 0 10px;
    background: #1ABC9C;
    color: #fff;
    border-radius: 4px;
    cursor: pointer;
}
.paybutton .insert:AFTER {
    content: "\f105";
    font: normal normal normal 14px/1 FontAwesome;
