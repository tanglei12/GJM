    font-size: 14px;
    font-family: -webkit-body, serif;
    cursor: pointer;
}
#viewTag .showTag {
    position: relative;
    padding: 0 14px;
}
#viewTag .showTag .fa-angle-down {
    position: absolute;
    top: 13px;
    right: 5px;
}
#viewTag .hideTag {
    display: none;
}
#viewTag .hideTag li {
    padding: 0 14px;
}
