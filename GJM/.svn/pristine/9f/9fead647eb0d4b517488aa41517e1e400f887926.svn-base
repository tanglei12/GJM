/*自定义checkbox*/
.checkbox-bill {
    position: relative;
    display: block;
    width: 34px;
    margin: 0 auto;
}
.checkbox-bill .input_check {
    display: none;
}
.checkbox-bill .input_check + span {
    display: inline-block;
    border: 1px solid #a9a9a9;
    width: 16px;
    height: 16px;
    position: absolute;
    top: 50%;
    left: 50%;
    margin-top: -8px;
    margin-left: -8px;
    border-radius: 2px;
    background-color: #FFF;
}
.checkbox-bill .input_check:checked + span {
    border: 1px solid #18bc9c
}
.checkbox-bill .input_check:checked + span:after {
    content: "";
    position: absolute;
    left: 0;
    bottom: 5px;
    width: 15px;
    height: 9px;
    border: 2px solid #18bc9c;
    border-top-color: transparent;
    border-right-color: transparent;
    -webkit-transform: rotate(-60deg);
    -moz-transform: rotate(-60deg);
    -ms-transform: rotate(-60deg);
    transform: rotate(-45deg);
}
/*自定义checkbox*/
.checkbox-a {
    position: relative;
    display: inline-block;
    width: 100%;
}
.checkbox-a .input_check {
    display: none;
}
.checkbox-a .input_check + span {
    display: block;
    border: 1px solid #a9a9a9;
    width: 14px;
    height: 14px;
    position: absolute;
    top: 8px;
    left: 0;
    background-color: #FFF;
    border-radius: 1px;
}
.checkbox-a .input_check:checked + span {
    border: 1px solid #21B0E2;
    background-color: #21B0E2;
}
.checkbox-a .input_check:checked + span:after {
    content: "";
    position: absolute;
    left: 0;
    bottom: 5px;
    width: 12px;
    height: 6px;
    border: 2px solid #FFF;
    border-top-color: transparent;
    border-right-color: transparent;
    -ms-transform: rotate(-60deg);
    -moz-transform: rotate(-60deg);
    -webkit-transform: rotate(-60deg);
    transform: rotate(-45deg);
}
.checkbox-a i {
    font-style: normal;
    margin-left: 20px;
}
/***************界面显示切换标签***********************/
#viewTag {
    position: absolute;
    float: left;
    width: 87px;
    height: 40px;
    line-height: 40px;
    margin-right: 10px;
    z-index: 1000;
}
#viewTag li {
    background: #1abc9c;
