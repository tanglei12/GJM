package com.gjp.model;

import java.util.Date;

/**
 * 房屋项目情况
 * 
 * @author 庆涛
 *
 */
public class UserCenterProjectSituation {

	// 房屋项目情况编码
	private Integer ps_id;
	// 房屋项目类型编号
	private String ps_type;
	// 房屋项目类型编号
	private String ps_moon;
	// 房屋项目名称编号
	private String ps_name;
	// 品牌
	private String pc_brand;
	// 品牌数量
	private Integer ps_num;
	// 0 新 1 旧
	private Integer ps_oldAndNew;
	// 0 好 1 坏
	private Integer ps_bol;
	// 房屋编码
	private String hi_code;
	// 继承者（前物业交接编号）
	private Integer ps_successor;
	// 物业交接编号
	private Integer transferSheet_id;
	// 创建时间
	private Date ps_createTime;
	// 配置状态
	private Integer ps_state;
	// 房屋项目编号
	private Integer ucp_id;

	// ==================扩展字段=====================
	private String RoomType_Name;// 房间名称
	private String ucp_name;// 项目名称

	public UserCenterProjectSituation() {
		super();
	}

	public Integer getUcp_id() {
		return ucp_id;
	}

	public Integer getPs_id() {
		return ps_id;
	}

	public void setPs_id(Integer ps_id) {
		this.ps_id = ps_id;
	}

	public String getPs_name() {
		return ps_name;
	}

	public void setPs_name(String ps_name) {
		this.ps_name = ps_name;
	}

	public String getPc_brand() {
		return pc_brand;
	}

	public void setPc_brand(String pc_brand) {
		this.pc_brand = pc_brand;
	}

	public Integer getPs_num() {
		return ps_num;
	}

	public void setPs_num(Integer ps_num) {
		this.ps_num = ps_num;
	}

	public Integer getPs_oldAndNew() {
		return ps_oldAndNew;
	}

	public void setPs_oldAndNew(Integer ps_oldAndNew) {
		this.ps_oldAndNew = ps_oldAndNew;
	}

	public Integer getPs_bol() {
		return ps_bol;
	}

	public void setPs_bol(Integer ps_bol) {
		this.ps_bol = ps_bol;
	}

	public void setUcp_id(Integer ucp_id) {
		this.ucp_id = ucp_id;
	}

	public String getRoomType_Name() {
		return RoomType_Name;
	}

	public void setRoomType_Name(String roomType_Name) {
		RoomType_Name = roomType_Name;
	}

	public String getUcp_name() {
		return ucp_name;
	}

	public void setUcp_name(String ucp_name) {
		this.ucp_name = ucp_name;
	}

	public String getHi_code() {
		return hi_code;
	}

	public void setHi_code(String hi_code) {
		this.hi_code = hi_code;
	}

	public Integer getTransferSheet_id() {
		return transferSheet_id;
	}

	public void setTransferSheet_id(Integer transferSheet_id) {
		this.transferSheet_id = transferSheet_id;
	}

	public String getPs_type() {
		return ps_type;
	}

	public void setPs_type(String ps_type) {
		this.ps_type = ps_type;
	}

	public String getPs_moon() {
		return ps_moon;
	}

	public void setPs_moon(String ps_moon) {
		this.ps_moon = ps_moon;
	}

	public Integer getPs_successor() {
		return ps_successor;
	}

	public void setPs_successor(Integer ps_successor) {
		this.ps_successor = ps_successor;
	}

	public Date getPs_createTime() {
		return ps_createTime;
	}

	public void setPs_createTime(Date ps_createTime) {
		this.ps_createTime = ps_createTime;
	}

	public Integer getPs_state() {
		return ps_state;
	}

	public void setPs_state(Integer ps_state) {
		this.ps_state = ps_state;
	}

}
