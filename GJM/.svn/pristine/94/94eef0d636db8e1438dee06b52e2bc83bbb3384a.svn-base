package com.gjp.model;

import java.util.Date;

/**
 * 房源调价、调价申请记录表
 * @author shenhx
 * 2017-05-25
 *
 */
public class PriceApplyRecord {
	
	// ID
	private Integer pa_id;
	// 记录编号
	private Integer cir_id;
	// 房源编号
	private String hi_code;
	// 申请价格
	private double apply_price;
	// 申请人
	private Integer apply_em_id;
	// 申请时间
	private Date apply_time;
	// 复核人
	private Integer review_em_id;
	// 复核时间
	private Date review_time;
	// 状态 0-待复核；1-已复核
	private Integer apply_status;
	// 拒绝原因
	private String refused_reason;
	// 审批结果0-拒绝；1-通过
	private Integer review_result;
	
	/* 扩展字段 */
	// 小区房号
	private String house_address;
	// 申请内容
	private String apply_content;
	// 申请人名称
	private String apply_em_name;
	// 复核人名称
	private String review_em_name;
	private Integer size;
	public Integer getPa_id() {
		return pa_id;
	}
	public void setPa_id(Integer pa_id) {
		this.pa_id = pa_id;
	}
	public Integer getCir_id() {
		return cir_id;
	}
	public void setCir_id(Integer cir_id) {
		this.cir_id = cir_id;
	}
	public String getHi_code() {
		return hi_code;
	}
	public void setHi_code(String hi_code) {
		this.hi_code = hi_code;
	}
	public double getApply_price() {
		return apply_price;
	}
	public void setApply_price(double apply_price) {
		this.apply_price = apply_price;
	}
	public Integer getApply_em_id() {
		return apply_em_id;
	}
	public void setApply_em_id(Integer apply_em_id) {
		this.apply_em_id = apply_em_id;
	}
	public Date getApply_time() {
		return apply_time;
	}
	public void setApply_time(Date apply_time) {
		this.apply_time = apply_time;
	}
	public Integer getReview_em_id() {
		return review_em_id;
	}
	public void setReview_em_id(Integer review_em_id) {
		this.review_em_id = review_em_id;
	}
	public Date getReview_time() {
		return review_time;
	}
	public void setReview_time(Date review_time) {
		this.review_time = review_time;
	}
	public Integer getApply_status() {
		return apply_status;
	}
	public void setApply_status(Integer apply_status) {
		this.apply_status = apply_status;
	}
	public String getHouse_address() {
		return house_address;
	}
	public void setHouse_address(String house_address) {
		this.house_address = house_address;
	}
	public String getApply_content() {
		return apply_content;
	}
	public void setApply_content(String apply_content) {
		this.apply_content = apply_content;
	}
	public String getApply_em_name() {
		return apply_em_name;
	}
	public void setApply_em_name(String apply_em_name) {
		this.apply_em_name = apply_em_name;
	}
	public String getReview_em_name() {
		return review_em_name;
	}
	public void setReview_em_name(String review_em_name) {
		this.review_em_name = review_em_name;
	}
	public String getRefused_reason() {
		return refused_reason;
	}
	public void setRefused_reason(String refused_reason) {
		this.refused_reason = refused_reason;
	}
	public Integer getReview_result() {
		return review_result;
	}
	public void setReview_result(Integer review_result) {
		this.review_result = review_result;
	}
	public Integer getSize() {
		return size;
	}
	public void setSize(Integer size) {
		this.size = size;
	}
	
}
