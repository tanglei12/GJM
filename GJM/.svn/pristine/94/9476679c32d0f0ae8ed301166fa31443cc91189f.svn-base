/*合同展示*/
.contractInfo {
    position: fixed;
    right: -18px;
    top: 60px;
    width: 80%;
    height: 100%;
    bottom: initial;
    border-top: 1px dashed #9fcac5;
    background-color: #FFF;
    box-shadow: 0 8px 15px 0 rgba(21, 20, 20, 2);
}
.contractInfo .contractInfo-refresh {
    position: absolute;
    top: 22px;
    right: 0px;
    width: 38px;
    height: 34px;
    cursor: pointer;
}
.contractInfo .contractInfo-close {
    position: absolute;
    top: 13px;
    right: 15px;
    width: 38px;
    height: 34px;
    cursor: pointer;
}
.contractInfo .contractInfo-close .fa-close {
    font-size: 18px;
    margin: 10px 13px;
}
.contractInfo dl {
    width: 100%;
    height: 45px;
    line-height: 45px;
}
.contractInfo dt {
    width: 80px;
    float: left;
    font-size: 13px;
    height: 45px;
    line-height: 45px;
    text-align: right;
}
.contractInfo dd {
    float: left;
    margin-left: 10px;
    font-size: 14px;
}
.contractInfo .contractInfo-title {
    border-bottom: 1px solid #3498db;
    height: 34px;
    line-height: 34px;
    font-size: 14px;
    text-indent: 5px;
    margin-bottom: 15px;
}
.contractInfo input {
    width: 180px;
    height: 32px;
    line-height: 32px;
    border: 0;
    border: 1px solid #CCC;
    text-indent: 7px;
    color: #666;
    font-size: 15px;
    background-color: #fdf7f7;
    border-radius: 3px;
}
#contract_inf span {
    min-width: 40%;
    min-height: 30px;
    line-height: 30px;
    float: left;
    font-size: 14px;
    color: #000;
}
#contract_inf label {
    min-width: 110px;
    padding-right: 12px;
    text-align: right;
    color: #666;
    font-size: 14px;
    /*font-weight: bold;*/
}
.hideTr {
    display: none;
}