package com.gjp.model;

import java.util.Date;

/**
 * 合作伙伴
 * 
 * @author zoe
 *
 */
public class PurchaseOrder {
	// 采购单ID
	private Integer pur_id;
	// 采购单唯一编码
	private String pur_code;
	// 创建时间
	private Date pur_addTime;
	// 审核人
	private Integer pur_audit_emid;
	// 审核时间
	private Date pur_audit_time;
	// 审核状态
	private Integer pur_type;
	// 审核意见
	private String pur_audit_suggestion;
	// 采购说明
	private String pur_specification;
	// 采购总价
	private Double pur_sumMoney;
	// 申请人
	private Integer pur_emId;
	// 采购人
	private Integer pur_purId;
	// 合作时间
	private Date pur_purTime;
	//开始位置
	private Integer start;
	//每页条数
	private Integer end;
	//采购单位置（0：公司；其余：房屋/采购单编号）
	private String pur_addres;
	//采购人名称
	private String user_name;
	
	
	//GET  and  SET
	public Integer getPur_id() {
		return pur_id;
	}
	public void setPur_id(Integer pur_id) {
		this.pur_id = pur_id;
	}
	public String getPur_code() {
		return pur_code;
	}
	public void setPur_code(String pur_code) {
		this.pur_code = pur_code;
	}
	public Date getPur_addTime() {
		return pur_addTime;
	}
	public void setPur_addTime(Date pur_addTime) {
		this.pur_addTime = pur_addTime;
	}
	public Integer getPur_audit_emid() {
		return pur_audit_emid;
	}
	public void setPur_audit_emid(Integer pur_audit_emid) {
		this.pur_audit_emid = pur_audit_emid;
	}
	public Date getPur_audit_time() {
		return pur_audit_time;
	}
	public void setPur_audit_time(Date pur_audit_time) {
		this.pur_audit_time = pur_audit_time;
	}
	public Integer getPur_type() {
		return pur_type;
	}
	public void setPur_type(Integer pur_type) {
		this.pur_type = pur_type;
	}
	public String getPur_audit_suggestion() {
		return pur_audit_suggestion;
	}
	public void setPur_audit_suggestion(String pur_audit_suggestion) {
		this.pur_audit_suggestion = pur_audit_suggestion;
	}
	public String getPur_specification() {
		return pur_specification;
	}
	public void setPur_specification(String pur_specification) {
		this.pur_specification = pur_specification;
	}
	public Double getPur_sumMoney() {
		return pur_sumMoney;
	}
	public void setPur_sumMoney(Double pur_sumMoney) {
		this.pur_sumMoney = pur_sumMoney;
	}
	public Integer getPur_emId() {
		return pur_emId;
	}
	public void setPur_emId(Integer pur_emId) {
		this.pur_emId = pur_emId;
	}
	public Integer getPur_purId() {
		return pur_purId;
	}
	public void setPur_purId(Integer pur_purId) {
		this.pur_purId = pur_purId;
	}
	public Date getPur_purTime() {
		return pur_purTime;
	}
	public void setPur_purTime(Date pur_purTime) {
		this.pur_purTime = pur_purTime;
	}
	public Integer getStart() {
		return start;
	}
	public void setStart(Integer start) {
		this.start = start;
	}
	public Integer getEnd() {
		return end;
	}
	public void setEnd(Integer end) {
		this.end = end;
	}
	public String getPur_addres() {
		return pur_addres;
	}
	public void setPur_addres(String pur_addres) {
		this.pur_addres = pur_addres;
	}
	public String getUser_name() {
		return user_name;
	}
	public void setUser_name(String user_name) {
		this.user_name = user_name;
	}
	
}
