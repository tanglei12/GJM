package com.gjp.model;

import java.math.BigDecimal;
import java.util.Date;

/**
 * 退款流水账单表
 * @author tanglei
 * @Date 2017年7月25日  上午11:10:55
 */
public class FinanceRefundFlowStatement {
	 //主键id
	 private Integer rf_id;
	 //退款流水号
	 private String rf_refundNumber;
	 //支付流水号
	 private String rf_serialNumber; 
	 //商户订单号
	 private String rf_orderNumber;
	 //支付平台交易号
	 private String rf_payTransNumber;
	 //房源CODE（商品号）
	 private String hi_code;
	 //流水类型（1：合同、2：服务、3：结算、4：定金）
	 private Integer rf_type;
	 //流水名称
	 private String rf_title;
	 //流水名称
	 private String rf_subtitle;
	 //流水类型（1：收款、2：付款）
	 private Integer rf_balPay;
	 //支付金额
	 private BigDecimal rf_money;
	 //支付状态（1：待付款、2：已付款、3：取消）
	 private Integer rf_state;
	 //支付方式（支付宝、微信）
	 private String rf_payType;
	 //支付时间
	 private Date rf_payTime;
	 // 收款方CODE
	 private String rf_payeeCode;
	 //收款方名
	 private String rf_payeeName;
	 //收款方账号
	 private String rf_payeeAccount;
	 //付款方CODE
	 private String rf_payerCode;
	 //付款方名
	 private String rf_payerName;
	 //付款方账号
	 private String rf_payerAccount;
	 //备注
	 private String rf_remark;
	 //创建时间
	 private Date rf_createTime;
	
	public Integer getRf_id() {
		return rf_id;
	}
	public void setRf_id(Integer rf_id) {
		this.rf_id = rf_id;
	}
	public String getRf_refundNumber() {
		return rf_refundNumber;
	}
	public void setRf_refundNumber(String rf_refundNumber) {
		this.rf_refundNumber = rf_refundNumber;
	}
	public String getRf_serialNumber() {
		return rf_serialNumber;
	}
	public void setRf_serialNumber(String rf_serialNumber) {
		this.rf_serialNumber = rf_serialNumber;
	}
	public String getRf_orderNumber() {
		return rf_orderNumber;
	}
	public void setRf_orderNumber(String rf_orderNumber) {
		this.rf_orderNumber = rf_orderNumber;
	}
	public String getRf_payTransNumber() {
		return rf_payTransNumber;
	}
	public void setRf_payTransNumber(String rf_payTransNumber) {
		this.rf_payTransNumber = rf_payTransNumber;
	}
	public String getHi_code() {
		return hi_code;
	}
	public void setHi_code(String hi_code) {
		this.hi_code = hi_code;
	}
	public Integer getRf_type() {
		return rf_type;
	}
	public void setRf_type(Integer rf_type) {
		this.rf_type = rf_type;
	}
	public String getRf_title() {
		return rf_title;
	}
	public void setRf_title(String rf_title) {
		this.rf_title = rf_title;
	}
	public String getRf_subtitle() {
		return rf_subtitle;
	}
	public void setRf_subtitle(String rf_subtitle) {
		this.rf_subtitle = rf_subtitle;
	}
	public Integer getRf_balPay() {
		return rf_balPay;
	}
	public void setRf_balPay(Integer rf_balPay) {
		this.rf_balPay = rf_balPay;
	}
	public BigDecimal getRf_money() {
		return rf_money;
	}
	public void setRf_money(BigDecimal rf_money) {
		this.rf_money = rf_money;
	}
	public Integer getRf_state() {
		return rf_state;
	}
	public void setRf_state(Integer rf_state) {
		this.rf_state = rf_state;
	}
	public String getRf_payType() {
		return rf_payType;
	}
	public void setRf_payType(String rf_payType) {
		this.rf_payType = rf_payType;
	}
	public Date getRf_payTime() {
		return rf_payTime;
	}
	public void setRf_payTime(Date rf_payTime) {
		this.rf_payTime = rf_payTime;
	}
	public String getRf_payeeCode() {
		return rf_payeeCode;
	}
	public void setRf_payeeCode(String rf_payeeCode) {
		this.rf_payeeCode = rf_payeeCode;
	}
	public String getRf_payeeName() {
		return rf_payeeName;
	}
	public void setRf_payeeName(String rf_payeeName) {
		this.rf_payeeName = rf_payeeName;
	}
	public String getRf_payeeAccount() {
		return rf_payeeAccount;
	}
	public void setRf_payeeAccount(String rf_payeeAccount) {
		this.rf_payeeAccount = rf_payeeAccount;
	}
	public String getRf_payerCode() {
		return rf_payerCode;
	}
	public void setRf_payerCode(String rf_payerCode) {
		this.rf_payerCode = rf_payerCode;
	}
	public String getRf_payerName() {
		return rf_payerName;
	}
	public void setRf_payerName(String rf_payerName) {
		this.rf_payerName = rf_payerName;
	}
	public String getRf_payerAccount() {
		return rf_payerAccount;
	}
	public void setRf_payerAccount(String rf_payerAccount) {
		this.rf_payerAccount = rf_payerAccount;
	}
	public String getRf_remark() {
		return rf_remark;
	}
	public void setRf_remark(String rf_remark) {
		this.rf_remark = rf_remark;
	}
	public Date getRf_createTime() {
		return rf_createTime;
	}
	public void setRf_createTime(Date rf_createTime) {
		this.rf_createTime = rf_createTime;
	}
	 
