package com.gjp.model;

import java.util.Date;

/**
 * 物品关系表
 * 
 * @author 秦莎
 *
 * @version 创建时间：2016年8月10日 上午14:58:31
 */
public class ItemsRelation {

	// 关系表ID
	private Integer ir_id;
	// 物品唯一编号
	private String inv_code;
	// 房屋编号
	private String hi_code;
	// 合同编号
	private String contractObject_code;
	// 绑定状态（1：绑定、0：解绑）
	private Integer ir_state;
	// 创建时间
	private Date ir_createTime;
	// 是否计算业绩（0：否、1：是）
	private String ir_isCalAchi;
	// 添置日期
	private Date ir_addDate;
	//物品类型
	private String inv_type;
	//物品名称
	private String inv_name;
	//物品单价
	private Double inv_price;
	
	public Integer getIr_id() {
		return ir_id;
	}
	public void setIr_id(Integer ir_id) {
		this.ir_id = ir_id;
	}
	public String getInv_code() {
		return inv_code;
	}
	public void setInv_code(String inv_code) {
		this.inv_code = inv_code;
	}
	public String getHi_code() {
		return hi_code;
	}
	public void setHi_code(String hi_code) {
		this.hi_code = hi_code;
	}
	public String getContractObject_code() {
		return contractObject_code;
	}
	public void setContractObject_code(String contractObject_code) {
		this.contractObject_code = contractObject_code;
	}
	public Integer getIr_state() {
		return ir_state;
	}
	public void setIr_state(Integer ir_state) {
		this.ir_state = ir_state;
	}
	public Date getIr_createTime() {
		return ir_createTime;
	}
	public void setIr_createTime(Date ir_createTime) {
		this.ir_createTime = ir_createTime;
	}
	public String getIr_isCalAchi() {
		return ir_isCalAchi;
	}
	public void setIr_isCalAchi(String ir_isCalAchi) {
		this.ir_isCalAchi = ir_isCalAchi;
	}
	public Date getIr_addDate() {
		return ir_addDate;
	}
	public void setIr_addDate(Date ir_addDate) {
		this.ir_addDate = ir_addDate;
	}
	public String getInv_type() {
		return inv_type;
	}
	public void setInv_type(String inv_type) {
		this.inv_type = inv_type;
	}
	public String getInv_name() {
		return inv_name;
	}
	public void setInv_name(String inv_name) {
		this.inv_name = inv_name;
	}
	public Double getInv_price() {
		return inv_price;
	}
	public void setInv_price(Double inv_price) {
		this.inv_price = inv_price;
	}
	
	
	
	
	
}
