package com.gjp.model;

import java.util.Date;

/**
 * 定价类型
 * @author chen
 *
 * @date Dec 21, 2016 3:06:37 PM 
 */
public class PriceSettingType {

	// 定价类型编码
	private Integer pst_id;
	// 类型名称
	private String pst_name;
	// 涨价金额
	private Double pst_money;
	// 房屋最小期限
	private Integer pst_houseMinYear;
	// 租赁最小期限
	private Integer pst_contractMinYear;
	// 0:否 1:是
	private Integer pst_bool;
	// 特殊条款
	private String pst_clause;
	// 备注
	private String pst_remark;
	// 创建时间
	private Date pst_date;
	
	public Integer getPst_id() {
		return pst_id;
	}
	public void setPst_id(Integer pst_id) {
		this.pst_id = pst_id;
	}
	public String getPst_name() {
		return pst_name;
	}
	public void setPst_name(String pst_name) {
		this.pst_name = pst_name;
	}
	public Integer getPst_bool() {
		return pst_bool;
	}
	public void setPst_bool(Integer pst_bool) {
		this.pst_bool = pst_bool;
	}
	public String getPst_remark() {
		return pst_remark;
	}
	public void setPst_remark(String pst_remark) {
		this.pst_remark = pst_remark;
	}
	public Date getPst_date() {
		return pst_date;
	}
	public void setPst_date(Date pst_date) {
		this.pst_date = pst_date;
	}
	public Double getPst_money() {
		return pst_money;
	}
	public void setPst_money(Double pst_money) {
		this.pst_money = pst_money;
	}
	public Integer getPst_houseMinYear() {
		return pst_houseMinYear;
	}
	public void setPst_houseMinYear(Integer pst_houseMinYear) {
		this.pst_houseMinYear = pst_houseMinYear;
	}
	public Integer getPst_contractMinYear() {
		return pst_contractMinYear;
	}
	public void setPst_contractMinYear(Integer pst_contractMinYear) {
		this.pst_contractMinYear = pst_contractMinYear;
	}
	public String getPst_clause() {
		return pst_clause;
	}
	public void setPst_clause(String pst_clause) {
		this.pst_clause = pst_clause;
	}
	
}
