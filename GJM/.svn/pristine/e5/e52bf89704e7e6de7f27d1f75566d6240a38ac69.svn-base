body {
    height: 100%;
    margin: 0;
    padding: 0;
    font-family: "微软雅黑";
    color: #fff;
    background: #F6F6F6;
    font-size: 15px;
}

a {
    text-decoration: none;
    color: #666;
}

li {
    margin: 0;
    padding: 0;
    list-style: none;
}

ul {
    margin: 0;
    padding: 0;
}

img {
    border: 0;
}

input select {
}

.center {
    background-color: #FFF;
    padding: 0 2%;
    margin-top: 14px;
    overflow: hidden;
    height: auto;
}

.center dl {
    height: 50px;
    line-height: 50px;
    margin: 0;
    display: flex;
    border-bottom: 1px solid #e8e8e8;
}

.center dt {
    float: left;
    width: 130px;
    margin-left: 12px;
    text-align: right;
    color: #333
}

.center dt label {
    float: left;
}

.center dt em {
    float: right;
    color: #FF6666;
    height: 50px;
    line-height: 54px;
    margin-right: 3px;
}

.center dd {
    float: left;
    margin-left: 2%;
    width: 100%;
    position: relative;

}

.center dl.bottom {
    border-bottom: none;
}

.fa-plus-square {
    color: #3498DB;
    float: right;
    margin-top: 11px;
    cursor: pointer;
}

.center dd select {
    height: 30px;
    line-height: 30px;
    appearance: none;
    -moz-appearance: none;
    -webkit-appearance: none;
    border: none;
    background-color: #FFF;
    padding-left: 77%;
    color: #8a8a8a;
    outline: none;
    font-size: 15px;
    padding: 0;
    padding-left: 59%;
    margin-right: 10px;
    text-align: right;
    outline: none;
}

