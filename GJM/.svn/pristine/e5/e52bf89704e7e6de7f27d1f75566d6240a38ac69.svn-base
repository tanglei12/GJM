}*/
.center dd .selectButton {
    height: 30px;
    line-height: 30px;
    width: 100px;
    background: #fff;
    border: 1px solid #F2F2F2;
    border-radius: 2px;
    color: #5a5a5a;
    font-size: 15px;
    outline: none;
    margin-left: 10px;
    text-align: center;
}

.center dd .selectButton-checked {
    height: 30px;
    line-height: 30px;
    width: 100px;
    background: #ff6666;
    border: 1px solid #ff6666;
    border-radius: 2px;
    color: #fff;
    font-size: 15px;
    outline: none;
    margin-left: 10px;
    text-align: center;
}

.center dd input.house_address {
    text-align: right;
}

.center .service_title {
    height: 50px;
    line-height: 50px;
    width: 100%;
    padding: 0 2%;
}

.center .service_edit {
    color: #999;
    font-size: 14px;
    float: right;
}

.center .service_title img {
    width: 25px;
    height: 25px;
    margin-top: 12px;
    margin-right: 5px;
    float: left;
}

.serviceContent {
    background-color: #FBFBFB;
    font-size: 13px;
    position: relative;
    margin-bottom: 5px;
}

.serviceContent dl {
    border-bottom: none;
    width: 90%;
    height: 35px;
    line-height: 35px;
}

.serviceContent .delete {
    width: 10%;
    height: 70px;
    line-height: 70px;
    background-color: #FF6666;
    color: #FFF;
    position: absolute;
    right: 0;
    top: 0;
    font-size: 14px;
    text-align: center;
}

.bottom_service {
    height: 56px;
    line-height: 56px;
    padding: 0 2%;
    bottom: 0;
    left: 0;
    width: 100%;
    background-color: #FFF;
}

.bottom_service .title {
    margin-left: 20%;
    color: #FF6666;
}

.bottom_service .money {
    color: #FF6666;
}

.bottom_service button {
    background-color: #ff6666;
    width: 94%;
    height: 42px;
    margin: 0% 3%;
    line-height: 42px;
    border: none;
    color: #FFF;
    font-size: 16px;
    border-radius: 4px;
}

.center.bottomMargin {
    margin-bottom: 60px;
}

.service_money {
    height: 40px;
    line-height: 40px;
}

.service_money .service_money_title {
    width: 74px;
    float: left;
    text-align: right;
}

.service_money .service_moneys {
    float: right;
    margin-right: 2%;
    color: #FF6666;
}

.font-icon {
    position: absolute;
    top: 3px;
    right: 0px;
    display: inline;
    width: 8px;
    margin: 18px 2px 0 10px;
    background: none !important;
    height: 8px;
    border: solid gray;
    border-width: 1px 1px 0 0;
    -webkit-transform: rotate(45deg);
    margin-right: 15px;
}

.center .textareaBox {
    clear: both;
    width: 93%;

}

.center .textareaBox textarea {
    font-family: "微软雅黑";
    width: 100%;
    color: #999;
    border: 1px solid #F2F2F2;
    resize: none;
    color: #5a5a5a;
    text-indent: 5px;
    padding-top: 5px;
}



textarea::-webkit-input-placeholder {
    color: #5a5a5a;
}

.serviceTBox {
    /* display: flex;*/
}

.serviceT {
    width: 57px;
    height: 85px;
    margin: 10px 0px 0px 10px;
    float: left;
}

.center .serviceT img {
    width: 40px;
    height: 40px;
    margin: auto;
}

.center .serviceT span {
    display: block;
    width: 57px;
    text-align: center;
    color: #d5d5d5;
    font-size: 14px;
}

select {
    direction: rtl;
}

select option {
    direction: ltr;
}

.content{
    height: auto;
}


.content-submmit {
    position: relative;
    display: block;
    flex: 1;
    line-height: 44px;
    font-size: 14px;
    text-align: center;
    border-radius: 3px;
    margin-left: 10px;
}
.content-submmit>i {
    font-size: 16px;
    position: relative;
    top: 1px;
    margin-right: 4px;
}
