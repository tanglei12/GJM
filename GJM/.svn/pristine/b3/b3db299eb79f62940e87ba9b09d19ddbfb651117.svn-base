.msg-box {
  position: absolute;
  top: 100%;
  left: 0;
  border: 2px solid #e74c3c;
  padding: 0 10px;
  line-height: 30px;
  border-radius: 4px;
  background: #e74c3c;
  font-size: 14px;
  color: #fff;
  cursor: default;
  white-space: nowrap;
  overflow: visible !important;
  z-index: 999;
}
