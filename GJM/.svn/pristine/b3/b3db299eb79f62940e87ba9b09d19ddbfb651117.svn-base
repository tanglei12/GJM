.common-borderbox {
  position: relative;
  border: 1px solid #ccc;
  color: #888;
  padding: 0 8px;
  font-size: 14px;
  cursor: pointer;
  border-radius: 3px;
  -moz-user-select: none;
  -webkit-user-select: none;
}
.common-borderbox-checked {
  border-color: #1ABC9C;
  color: #1ABC9C;
}
.common-borderbox-disabled {
  background: #ccc;
  cursor: not-allowed;
}
.common-borderbox-checked:BEFORE {
  content: "\f058";
  font: normal normal normal 14px / 1 FontAwesome;
  position: absolute;
  right: 1px;
  bottom: 0;
  z-index: 1;
}
.common-borderbox-add {
  position: relative;
  border: 2px solid #1ABC9C;
  color: #ffffff;
  padding: 0 16px;
  font-size: 14px;
  display: block;
  float: left;
  height: 36px;
  margin-right: 12px;
  cursor: pointer;
  border-radius: 4px;
  background: #1ABC9C;
}
/**/
input.form-control,
select.form-control,
textarea.form-control {
  height: 42px;
  line-height: 42px;
  padding: 0 10px;
  font-size: 14px;
  color: #555;
  background: #ffffff;
  border: 1px solid #e4e4e4;
  border-radius: 3px;
  -webkit-appearance: none;
}
input.form-control:read-only,
select.form-control:read-only,
textarea.form-control:read-only {
  background-color: #f7f7f7;
  cursor: pointer;
}
input.form-control:disabled,
select.form-control:disabled,
textarea.form-control:disabled {
  background-color: #e6e6e6;
}
input.form-control:focus,
select.form-control:focus,
textarea.form-control:focus {
  border-color: #3498DB;
  outline: 0;
}
/**/
textarea.form-control {
  border: none;
}
/**/
input.form-control:-moz-placeholder {
  color: #999;
}
input.form-control:-ms-input-placeholder {
  color: #999999;
}
input.form-control::-webkit-input-placeholder {
  color: #999999;
}
/**/
button.form-control {
  position: relative;
  line-height: 46px;
  width: 100%;
  font-size: 14px;
  border-radius: 3px;
}
