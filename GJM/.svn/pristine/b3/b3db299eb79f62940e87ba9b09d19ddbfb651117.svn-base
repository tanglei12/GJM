button.list-item {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
}
button.list-item:enabled {
  -webkit-transition: all;
  transition: all;
  -webkit-transition-timing-function: linear;
  transition-timing-function: linear;
  -webkit-transition-duration: .2s;
  transition-duration: .2s;
}
button.list-item:enabled:active {
  background: rgba(0, 0, 0, 0.2);
}
.switch-button {
  position: relative;
  display: inline-block;
  font-weight: normal;
  font-size: 12px;
  padding: 0 10px;
  line-height: 24px;
  border: 1px solid #ddd;
  border-radius: 14px;
  white-space: nowrap;
}
.switch-button > input {
  display: none;
}
.switch-button-on > i {
  position: relative;
  top: 2px;
  left: -7px;
  width: 20px;
  height: 20px;
  display: inline-block;
  float: left;
  background: #ddd;
  border-radius: 50%;
}
.switch-button-off > i {
  position: relative;
  top: 2px;
  right: -7px;
  width: 20px;
  height: 20px;
  display: inline-block;
  float: right;
  background: #ddd;
  border-radius: 50%;
}
