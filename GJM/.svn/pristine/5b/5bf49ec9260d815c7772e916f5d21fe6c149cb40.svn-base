package com.gjp.model;

import java.util.Date;

/**
 * 意向房源跟进表
 * 
 * @author zoe
 *
 */
public class HouseFollow {

	// 房源跟进编码
	private Integer ghf_id;
	// 跟进项目
	private String ghf_item;
	// 具体内容
	private String ghf_content;
	// 意向房源编号
	private Integer phi_id;
	// 内部职员编号
	private Integer em_id;
	// 提交时间
	private Date ghf_time;
	// 跟进流程
	private String ghf_state;
	// 提交时间
	private Date ghf_date;
	//扩展字段
	// 提交时间
	private String em_name;
	//房屋code
	private String hi_code;
	
	public String getHi_code() {
		return hi_code;
	}

	public void setHi_code(String hi_code) {
		this.hi_code = hi_code;
	}

	public HouseFollow() {
		super();
	}

	public Integer getGhf_id() {
		return ghf_id;
	}

	public void setGhf_id(Integer ghf_id) {
		this.ghf_id = ghf_id;
	}

	public Integer getPhi_id() {
		return phi_id;
	}

	public void setPhi_id(Integer phi_id) {
		this.phi_id = phi_id;
	}

	public Integer getEm_id() {
		return em_id;
	}

	public void setEm_id(Integer em_id) {
		this.em_id = em_id;
	}

	public Date getGhf_time() {
		return ghf_time;
	}

	public void setGhf_time(Date ghf_time) {
		this.ghf_time = ghf_time;
	}

	public String getEm_name() {
		return em_name;
	}

	public void setEm_name(String em_name) {
		this.em_name = em_name;
	}

	public String getGhf_item() {
		return ghf_item;
	}

	public void setGhf_item(String ghf_item) {
		this.ghf_item = ghf_item;
	}

	public String getGhf_content() {
		return ghf_content;
	}

	public void setGhf_content(String ghf_content) {
		this.ghf_content = ghf_content;
	}

	public String getGhf_state() {
		return ghf_state;
	}

	public void setGhf_state(String ghf_state) {
		this.ghf_state = ghf_state;
	}

	public Date getGhf_date() {
		return ghf_date;
	}

	public void setGhf_date(Date ghf_date) {
		this.ghf_date = ghf_date;
	}

	
}
