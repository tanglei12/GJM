body{
	margin:0;
	padding:0;
	font-family:"Comic Sans MS","幼圆","黑体",sans-serif;
  	color: #666;
   	background: #fff;
	}
a{
	text-decoration:none;
	color: #666;
}
li{
	margin:0;
	padding:0;
	list-style:none;
	}
ul{
	margin:0;
	padding:0;
	}
img{
	border:0;
	}
dl{
	margin:0;
	width:100%;
	overflow:hidden;
	font-size:0.9rem;
	margin:5px 0;
}

.content{
	width: 100%;
	overflow: hidden;
}
.content .houseList{
	margin-left: 5%;
	height: 50px;
	width: 95%;
    border-bottom: 1px solid #e5e5e5;
}
.content .houseList button{
	width: 100%;
	height: 100%;
	background: transparent;
	border: none;
	text-align: left;
	padding: 0;
}
.content .houseList button img{
	width: 5%;
	margin-top: 5px;
	float: left;
}
.content .houseList button label{
	float: left;
	margin-top: 5px;
	margin-left: 15px;
	color: #666;
}
.content .houseList button i{
	display: block;
	width: 15px;
	height: 26px;
	background: url("/resources/image/appPage/right_icon.png") no-repeat;
	float: right;
	margin-right: 10px;
}

button{
	outline: none;
}