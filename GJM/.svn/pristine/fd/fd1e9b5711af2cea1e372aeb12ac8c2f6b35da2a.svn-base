/**=========select=E=======*/
/**=========自定义select=S=======*/
dl.select {
    position: relative;
    width: 120px;
    height: 36px;
    float: left;
    margin-right: 10px;
}

dl.select:HOVER > dd {
    display: block;
}

dt.selected {
    position: relative;
    height: 32px;
    line-height: 32px;
    font-size: 14px;
    border: 2px solid #1ABC9C;
    text-indent: 20px;
    text-align: left;
    border-radius: 4px;
}

dt.selected:BEFORE {
    content: "";
    display: block;
    border-style: solid;
    border-width: 8px 8px 0px 8px;
    border-color: #1ABC9C transparent transparent transparent;
    height: 0px;
    position: absolute;
    right: 8px;
    top: 15px;
    width: 0px;
    -webkit-transform: rotate(360deg);
    z-index: 200;
}

dd.option {
    position: relative;
    display: none;
    font-size: 14px;
    padding-top: 10px;
    position: relative;
    z-index: 100;
    text-indent: 19px;
    text-align: left;
}

dd.option:BEFORE {
    content: "";
    display: block;
    border-style: solid;
    border-width: 0 9px 9px 9px;
    border-color: transparent transparent #1ABC9C transparent;
    height: 0px;
    position: absolute;
    right: 8px;
    top: 3px;
    width: 0px;
    -webkit-transform: rotate(360deg);
    z-index: 200;
}

dd.option > ul {
    position: relative;
    list-style-type: none;
    border-top: 2px solid #1ABC9C;
    padding: 2px;
    padding-top: 0;
    background: #1ABC9C;
    border-radius: 4px;
}

dd.option > ul > li {
    display: block;
    height: 30px;
    line-height: 30px;
    font-size: 14px;
    position: relative;
    z-index: 100;
    background: #f7f7f7;
    border-radius: 4px;
    margin-bottom: 2px;
    cursor: pointer;
}

