    height: 45px;
    line-height: 45px;
    border: 1px solid #ededed;
    padding: 0 15px;
}

.payBill select, input {
    background: #FFF;
}

.payBill select {
    float: left;
    height: 32px;
    line-height: 32px;
    border: 1px solid #ddd;
    margin-top: 6px;
}

.payBill input {
    display: block;
    height: 32px;
    line-height: 32px;
    background-color: #fdf7f7;
    text-align: center;
    color: #666;
    float: left;
    border: 1px solid #ddd;
    margin-top: 6px;
}

.payBill .payType {
    position: relative;
    float: left;
    display: inline-block;
    height: 32px;
    line-height: 30px;
    padding: 0 8px;
    margin-top: 6px;
    font-size: 12px;
    border-top-left-radius: 3px;
    border-bottom-left-radius: 3px;
    text-align: center;
    border: 1px solid #ddd;
    border-right: none;
    color: #34495e;
    background: #fbfbfb;
}

.payBill .paySubmit {
    padding: 0;
    margin-top: 6px;
    height: 32px;
    line-height: 32px;
    border: none;
    color: #FFF;
    background: #1ABC9C;
    line-height: 30px;
    padding: 0 15px;
    float: left;
    border-top-right-radius: 3px;
    border-bottom-right-radius: 3px;
    border-radius: 0;
}

.payTable {
    width: 480px;
    border-collapse: collapse;
    border-spacing: 0;
}

.payTable td {
    text-align: center;
    border: 1px solid #ddd;
}

