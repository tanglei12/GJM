package com.gjp.model;

import java.util.Date;

/**
 * 客户需求表
 * 
 * @author 陈智颖
 *
 * @version 创建时间：2016年8月1日 下午2:55:36 
 */
public class UserCustomerDemand {
	
	// 客户需求编码
	private Integer ccd_id;
	// 客户需求
	private String ccd_content;
	// 需求时间
	private Date ccd_date;
	// 时间
	private Date ccd_time;
	// 备注
	private String ccd_text;
	// 客户编码
	private Integer cc_id;
	
	public UserCustomerDemand(){
		
	}

	public Integer getCcd_id() {
		return ccd_id;
	}

	public void setCcd_id(Integer ccd_id) {
		this.ccd_id = ccd_id;
	}

	public String getCcd_content() {
		return ccd_content;
	}

	public void setCcd_content(String ccd_content) {
		this.ccd_content = ccd_content;
	}

	public Date getCcd_date() {
		return ccd_date;
	}

	public void setCcd_date(Date ccd_date) {
		this.ccd_date = ccd_date;
	}

	public Date getCcd_time() {
		return ccd_time;
	}

	public void setCcd_time(Date ccd_time) {
		this.ccd_time = ccd_time;
	}

	public String getCcd_text() {
		return ccd_text;
	}

	public void setCcd_text(String ccd_text) {
		this.ccd_text = ccd_text;
	}

	public Integer getCc_id() {
		return cc_id;
	}

	public void setCc_id(Integer cc_id) {
		this.cc_id = cc_id;
	}
	
}
