@charset "utf-8";
/* CSS Document */

body{
	margin:0;
	padding:0;
	font: 12px/150% Arial,Verdana,"\5b8b\4f53";
  	color: #666;
 	background: #fff;
	}
a{
	text-decoration:none;
	color: #666;
}
li{
	margin:0;
	padding:0;
	list-style:none;
	}
ul{
	margin:0;
	padding:0;
	}
img{
	border:0;
	}
select{
	width: 179px;
	height: 34px;
	border-radius: 4px;
	border: 1px solid #ccc;
	text-indent: 5px;
}
#orders-header {
    position: relative;
    height: auto;
    background: #ffffff;
    padding: 20px;
    margin-bottom: 20px;
}

.tenantCenter{
