@charset "utf-8";
/* CSS Document */
*{
	margin: 0;
	padding: 0;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
}
body {
	font: 12px/150% Arial, Verdana, "\5b8b\4f53";
	color: #666;
	background: #fff;
}
a {
	text-decoration: none;
	color: #666;
}

ul,li {
	list-style: none;
}
ul:AFTER{
	content: "";
	display: block;
	visibility: hidden;
	clear: both;
}
img {
	border: 0;
	display: block;
}
.error{color:#E74C3C !important;}
.tips{color:#F39C12 !important;}
.ok{color:#27AE60 !important;}
.next{color:#3498DB !important;}
.table-bg {
	height: 200px;
	background-image: url("../image/notdata.jpg");
	background-repeat: no-repeat;
	background-position: center;
}