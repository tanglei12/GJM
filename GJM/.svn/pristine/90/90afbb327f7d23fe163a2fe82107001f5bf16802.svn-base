.item-menu {position: relative;float: left;width: 100%; border-radius: 2px;}
.item-menu .menuB{position: relative;height: 34px;line-height: 34px;width: 100%;display: table;border-bottom: 1px solid #ddd;}
.item-menu .menuB>span{position: relative;display: table-cell;text-align: center;font-weight: bold;}
.item-menu .menu-content{position: relative;height: 30px;line-height: 30px;width: 100%;display: table;/* border-bottom: 1px solid #ddd; */}
.item-menu .menu-content:last-child{border-bottom:none;}
.item-menu .menu-content>span{position: relative;display: table-cell;text-align: center;vertical-align: middle;}
.item-menu .menu-content>span input.menu-content-input{position: relative;border-bottom: 1px solid #ddd;width: 80%;text-align: center;outline: none;padding: 2px 0;}
.check-item{position: relative; width: 80%;height: 22px;background: #fff;display: block;margin: 0 auto;border-radius: 11px;cursor: pointer; -moz-user-select: none;-webkit-user-select: none;}
.check-item-well{border: 1px solid #1ABC9C;}
.check-item-well:BEFORE{content: "";position: absolute;top: 1px;right: 2px;width: 18px;height: 18px;background: #1ABC9C;border-radius: 50%;}
.check-item-well>span{position: absolute;top: 0;left: 9px;height: 20px;display: block;color: #1ABC9C;line-height: 20px;}
.check-item-bad{border: 1px solid #E74C3C;}
.check-item-bad:BEFORE{content: "";position: absolute;top: 1px;left: 2px;width: 18px; height: 18px; background: #E74C3C; border-radius: 50%;}
.check-item-bad>span{position: absolute;top: 0;right: 9px;height: 20px;display: block;color: #E74C3C;line-height: 20px;}
.check-item-old{border: 1px solid #F39C12;}
.check-item-old:BEFORE{content: "";position: absolute;top: 1px;left: 2px;width: 18px;height: 18px;background: #F39C12;border-radius: 50%;}
.check-item-old>span{position: absolute;top: 0;right: 9px;height: 20px;display: block;color: #F39C12;line-height: 20px;}
.check-item>input[type="checkbox"]{display: none;height: 0;}
/***/
.item-list {position: relative;width: 100%;float: right;border-radius: 2px;}
.item-list .menuB{position: relative;height: 34px;line-height: 34px;width: 100%;display: table;font-weight: bold;}
.item-list .menuB>span{position: relative;display: table-cell;text-align: center;vertical-align: middle;}
.item-list .menu-content{position: relative;height: 34px;line-height: 34px;width: 100%;display: table;border-bottom: 1px solid #ddd;}
.item-list .menu-content>span{position: relative;display: table-cell;text-align: center;vertical-align: middle;}
