dd.option>ul {
    position: relative;
    list-style-type: none;
    background: #fff;
}

dd.option>ul>li {
    position: relative;
    display: block;
    height: 34px;
    line-height: 34px;
    font-size: 14px;
    z-index: 100;
    background: #fff;
    border: 1px solid #1ABC9C;
    margin-bottom: 4px;
    cursor: pointer;
    border-radius: 2px;
}
dd.option>ul>li:HOVER{
	background: #1ABC9C;
	color: #fff;
}
