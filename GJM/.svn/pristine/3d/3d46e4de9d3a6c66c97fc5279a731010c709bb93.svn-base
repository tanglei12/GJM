public class UserCustomerType {

	// 客户类别编码
	private Integer cctp_id;
	// 客户类别
	private String cctp_type;
	// 客户编码
	private Integer cc_id;
	
	public UserCustomerType(){
		
	}

	public Integer getCctp_id() {
		return cctp_id;
	}

	public void setCctp_id(Integer cctp_id) {
		this.cctp_id = cctp_id;
	}

	public String getCctp_type() {
		return cctp_type;
	}

	public void setCctp_type(String cctp_type) {
		this.cctp_type = cctp_type;
	}

	public Integer getCc_id() {
		return cc_id;
	}

	public void setCc_id(Integer cc_id) {
		this.cc_id = cc_id;
	}
	
}
