body{
	margin:0;
	padding:0;
	font-family:"Comic Sans MS","幼圆","黑体",sans-serif;
  	color: #666;
   	background: #F8F8F8;
   	font-size: 15px;
	}
a{
	text-decoration:none;
	color: #666;
}
li{
	margin:0;
	padding:0;
	list-style:none;
	}
ul{
	margin:0;
	padding:0;
	}
img{
	border:0;
	}
	
.center{
	background-color: #FFF;
	padding: 0 2%;
	margin-top: 10px;
	overflow: hidden;
	height: auto;
}
.center dl{
	height: 50px;
	line-height: 50px;
	margin: 0;
	display: flex;
	border-bottom: 1px solid #e8e8e8;
}
.center dt{
	float: left;
	width: 100px;
	text-align: right;
}
.center dt label{
	float: right;
}
.center dt em{
	float: right;
	color: #FF6666;
	height: 50px;
	line-height: 54px;
	margin-right: 3px;
}
.center dd{
	float: left;
	margin-left: 2%;
	width: 100%;
	position: relative;
}
.center dl.bottom{
	border-bottom: none;
}
.fa-plus-square{
	color: #3498DB;
	float: right;
    margin-top: 11px;
    cursor: pointer;
}
.center dd select{
    height: 30px;
    line-height: 30px;
	appearance:none;
	-moz-appearance:none;
	-webkit-appearance:none;
	border: none;
	background-color: #FFF;
	padding-left: 77%;
	color: #000;
	outline: none;
	font-size: 13px;
	padding:0;
	padding-left: 59%;
}
.center dd input{
	height: 30px;
	line-height: 30px;
	width: 90%;
	border: none;
	text-indent: 5px;
	background:#fff;
}
.center dd input.house_address{
	text-align: right;
}

.center .service_title{
	height: 50px;
	line-height: 50px;
	width: 100%;
	padding: 0 2%;
}
.center .service_edit{
	color: #999;
	font-size: 14px;
	float: right;
}
.center .service_title img{
	width: 25px;
	height: 25px;
	margin-top: 12px;
	margin-right: 5px;
	float: left;
}
.serviceContent{
	background-color: #FBFBFB;
	font-size: 13px;
	position: relative;
	margin-bottom: 5px;
}
.serviceContent dl{
	border-bottom: none;
	width: 90%;
	height: 35px;
	line-height: 35px;
}
.serviceContent .delete{
	width: 10%;
	height: 70px;
	line-height: 70px;
	background-color: #FF6666;
	color: #FFF;
	position: absolute;
	right:0;
	top:0;
	font-size: 14px;
	text-align: center;
}
.bottom_service{
	height: 60px;
	line-height: 60px;
	position: fixed;
	bottom: 0;
	left: 0;
	width: 100%;
	background-color: #FFF;
	border-top: 1px solid #e8e8e8;
}
.bottom_service .title{
	margin-left: 14%;
}
.bottom_service .money{
	color: #FF6666;
}
.bottom_service button{
	background-color: #ff6666;
	width: 100%;
	height: 60px;
	line-height: 60px;
	float: right;
	border: none;
	color:#FFF;
	font-size: 16px;
}
.center.bottomMargin{
	margin-bottom: 60px;
}

.service_money{
	height: 40px;
	line-height: 40px;
}
.service_money .service_money_title{
	width: 74px;
	float: left;
	text-align: right;
}
.service_money .service_moneys{
	float: right;
	margin-right: 2%;
	color: #FF6666;
}

.font-icon {
    position: absolute;
    top: 3px;
    right: 0px;
    display: inline;
    width: 8px;
    margin: 18px 2px 0 10px;
    background: none!important;
    height: 8px;
    border: solid gray;
    border-width: 1px 1px 0 0;
    -webkit-transform: rotate(45deg);
    margin-right: 15px;
}