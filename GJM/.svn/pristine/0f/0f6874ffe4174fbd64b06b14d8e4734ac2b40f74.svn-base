body {
	margin: 0;
	padding: 0;
	font-family: "Comic Sans MS", "幼圆", "黑体", sans-serif;
	color: #666;
	background: #fff;
}

a {
	text-decoration: none;
	color: #666;
}

li {
	margin: 0;
	padding: 0;
	list-style: none;
}

ul {
	margin: 0;
	padding: 0;
}

img {
	border: 0;
}

input, button, fieldset, textarea, select {
	outline: none;
}

dl {
	margin: 0;
	width: 100%;
	overflow: hidden;
	font-size: 0.9rem;
	margin: 5px 0;
}

.content-text {
	width: 100%;
	overflow: hidden;
	background-color: #FFF;
}

.buttonIcon button {
	width: 100%;
	height: 100%;
	position: absolute;
	left: 0;
	top: 0;
	border: none;
	background: transparent;
}

.buttonIcon {
	width: 33.1%;
	height: 110px;
	float: left;
	position: relative;
}

.buttonIcon_left {
	border-bottom: 1px solid #e7e7e5;
	border-right: 1px solid #e7e7e5;
}

.buttonIcon_top {
	border-top: 1px solid #e7e7e5;
}

.buttonIcon_center {
	border-bottom: 1px solid #e7e7e5;
}

.buttonIcon_right {
	border-bottom: 1px solid #e7e7e5;
	border-left: 1px solid #e7e7e5;
}

.buttonIcon img {
	width: 35%;
	text-align: center;
	padding: 0 32%;
	margin-top: 25px;
}

.buttonIcon label {
	display: block;
	text-align: center;
	font-size: 3.3vw;
	margin-top: -3px;
}

button{
	outline: none;
}