@charset "utf-8";
/* CSS Document */

	.jjicon{
		width:25px;
		height:25px;
		}
	.jjicon #jjicon{
		display:block;
		width:25px;
		height:25px;
		background:url(../image/-+icon.png) no-repeat;
		background-position:-20px 2px;
		}
	#opens{
		width:100%;
		padding:0;
		margin:0;
		display:none;
		margin-top:-5px;
		background-color:#FFF;
		}
	#opens #title{
		border-left: 0;
		width: auto;
		overflow: hidden;
		}
	#opens ul{
		color: #557386;
		display: block;
		background: #f0f6fa;
		border-left: solid 2px #c4d5df;
		margin:1px 0;
		height: auto;
		overflow: hidden;
		}
	#opens ul li{
		padding: 0 15px;
		float: left;
		}

	.button{
		width:90px;
		height:30px;
		line-height:30px;
		background-color:#3498DB;
		text-align:center;
		float:right;
		margin-top:40px;
		}
	.button a{
		display:block;
		width:100%;
		height:100%;
		color:#FFF;
		}