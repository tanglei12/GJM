package com.gjp.model;

import java.util.Date;
import java.util.List;

/**
 * 账单类型对象
 * 
 * @author 庆涛
 *
 */
public class BillTypeVo {

	// 类型ID
	private Integer bt_id;
	// 类型编号
	private Integer bt_code;
	// 类型名称
	private String bt_name;
	// 类型值
	private String bt_value;
	// 父级
	private Integer bt_parentCode;
	// 排序（0~99倒序排列）
	private Integer bt_order;
	// 备注
	private String bt_remark;
	// 创建时间
	private Date bt_createTime;
	// 类型编号
	private String type;
	// 类型编号
	private List<Integer> types;
	// 是否选中
	private boolean checked;

	public Integer getBt_id() {
		return bt_id;
	}

	public void setBt_id(Integer bt_id) {
		this.bt_id = bt_id;
	}

	public Integer getBt_code() {
		return bt_code;
	}

	public void setBt_code(Integer bt_code) {
		this.bt_code = bt_code;
	}

	public String getBt_name() {
		return bt_name;
	}

	public void setBt_name(String bt_name) {
		this.bt_name = bt_name;
	}

	public String getBt_value() {
		return bt_value;
	}

	public void setBt_value(String bt_value) {
		this.bt_value = bt_value;
	}

	public Integer getBt_parentCode() {
		return bt_parentCode;
	}

	public void setBt_parentCode(Integer bt_parentCode) {
		this.bt_parentCode = bt_parentCode;
	}

	public Integer getBt_order() {
		return bt_order;
	}

	public void setBt_order(Integer bt_order) {
		this.bt_order = bt_order;
	}

	public String getBt_remark() {
		return bt_remark;
	}

	public void setBt_remark(String bt_remark) {
		this.bt_remark = bt_remark;
	}

	public Date getBt_createTime() {
		return bt_createTime;
	}

	public void setBt_createTime(Date bt_createTime) {
		this.bt_createTime = bt_createTime;
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

	public List<Integer> getTypes() {
		return types;
	}

	public void setTypes(List<Integer> types) {
		this.types = types;
	}

	public boolean isChecked() {
		return checked;
	}

	public void setChecked(boolean checked) {
		this.checked = checked;
	}
}
