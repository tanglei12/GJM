@charset "utf-8";
/* CSS Document */
*{margin: 0;padding: 0;}
html,body{height: 100%;}
body {
	font: 12px/150% Arial, Verdana, "\5b8b\4f53";
	color: #666;
	background: #f2f4f8;
/* 	overflow-y:hidden; */
}
a {
	text-decoration: none;
	color: #666;
}

li {
	list-style: none;
}
img {
	border: 0;
	display: block;
}
.welcome_center{
	width: 100%;
	 position: relative;
}
.welcome_center .people{
	background-color: #FFF;
	width: 59%;
	margin-left: 1%;
	margin-top: 1%;
	float: left;
	position: relative;
	box-shadow: 0 1px 3px 0 rgba(0,0,0,.14);
}
.welcome_center a{
	width: 40px;
    height: 40px;
    top: 0;
    right: 0;
    position: absolute;
    display: block;
}
.title_tongji{
	width: 100%;
	display: block;
	height: 40px;
	line-height: 40px;
	text-indent: 15px;
    font-size: 14px;
	border-bottom: 1px solid #efefef;
}
.contents{
	width: 100%;
    overflow: hidden;
    height: 240px;
}
.contents .content_div{
	float: left;
    width: 19.5%;
}
.contents .content_line{
	float: left;
    width: 1px;
    height: 100px;
    background-color: #E2E2E2;
    margin-top: 75px;
}
.contents .content_div .content_title{
	margin-left: 15px;
    color: #575757;
    font-size: 14px;
    margin-top: 15px;
}
.contents .content_div .content_chart{
    overflow: hidden;
}
.quans .g-guide{
    margin-left: -5px;
}
.zheng .g-guide{
    margin-left: -90px;
}
.biil_water_count{
    width: 212px;
    font-size: 16px;
    border-top: 1px solid #E2E2E2;
    padding-top: 10px;
    color: #575757;
    font-family: "微软雅黑";
    margin-top: 10px;
}
.welcome_center .achievement{
	background-color: #FFF;
	width: 38%;
	margin-left: 1%;
	margin-top: 1%;
	float: left;
	position: relative;
	box-shadow: 0 1px 3px 0 rgba(0,0,0,.14);
}
.welcome_center .bill_tongji{
	width: 100%;
    overflow: hidden;
}
.welcome_center .bill_tongji .title_head{
    height: 65px;
}
.welcome_center .bill_tongji .title_head .title_head_tag{
    height: 35px;
    margin-top: 15px;
    margin-left: 15px;
    float: left;
}
.welcome_center .bill_tongji .title_head .title_head_tag li{
    float: left;
    width: 70px;
    height: 35px;
    line-height: 35px;
    text-align: center;
    border: 1px solid #E2E2E2;
    font-size: 14px;
    position: absolute;
    z-index: 0;
    -webkit-user-select:none;
    -moz-user-select:none;
    -ms-user-select:none;
    user-select:none;
}
.welcome_center .bill_tongji .title_head .title_head_tag li.click{
    border-color: #1ABC9C;
    z-index: 2;
    cursor: default;
    color: #1ABC9C;
}
.welcome_center .bill_tongji .title_head .title_head_select{
    float: right;
}
.welcome_center .bill_tongji .title_head .title_head_select select{
    background-color: #FFFFFF;
    height: 35px;
    line-height: 35px;
    width: 100px;
    border: 1px solid #E2E2E2;
    margin-top: 15px;
    margin-right: 15px;
}

.welcome_center .business{
	background-color: #FFF;
	width: 98%;
	margin-left: 1%;
	margin-top: 1%;
	float: left;
	position: relative;
	box-shadow: 0 1px 3px 0 rgba(0,0,0,.14);
}
.welcome_center .business_center{
	width: 94%;
	padding: 1% 3%;
	height: auto;
	overflow:hidden;
	position: relative;
}
.welcome_center .business_center .business_center_div{
	width: 12%;
	height: 50px;
	line-height: 50px;
	float: left;
	text-align: center;
	color: #FFF;
	font-size: 16px;
	float: left;
}
.welcome_center .business_center .business-JP{
	display: inline-block;
    font: normal normal normal 14px/1 FontAwesome;
    font-size: inherit;
    text-rendering: auto;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
    transform: translate(0, 0);
    color: #004775;
    margin-left: 1.5%;
    float: left;
}
.welcome_center .business_center .business-JP:before{
	content: "\f105";
	font-size: 50px;
}

.title_lable{
	display:block;
	width: 15%;
	text-align:center;
	text-indent:0;
	height:100%;
	background-color: #FFF;
	color:#666;
	float: left;
}
.lableClick{
	color:#FFF;
	background-color: #33d176!important;
}

.timeLine_message{
	overflow: hidden;
}
.timeline{
	position: relative;
    padding: 20px 0 20px;
    list-style: none;
    overflow: hidden;
}
.timeline > li.timeline-inverted > .timeline-panel {
    float: right;
}
.timeline > li.timeline-inverted > .timeline-panel:after {
    right: auto;
    left: -14px;
    border-right-width: 14px;
    border-left-width: 0;
}
.timeline > li.timeline-inverted > .timeline-panel:before {
    right: auto;
    left: -15px;
    border-right-width: 15px;
    border-left-width: 0;
}
.timeline li{
	overflow: hidden;
	padding: 0 3%;
}
.timeline:before {
    content: " ";
    position: absolute;
    top: 0;
    bottom: 0;
    left: 50%;
    width: 3px;
    margin-left: -1.5px;
    background-color: #eeeeee;
}

.timeline > li {
    position: relative;
    margin-bottom: 20px;
}
.timeline > li {
    position: relative;
    margin-bottom: 20px;
}
.timeline .fa {
    display: inline-block;
    font: normal normal normal 14px/1 FontAwesome;
    font-size: inherit;
    text-rendering: auto;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
    transform: translate(0, 0);
}
.timeline .fa-check:before {
    content: "\f00c";
    color: #ccc;
}
.timeline > li > .timeline-badge {
    z-index: 100;
    position: absolute;
    top: 16px;
    left: 50%;
    width: 50px;
    height: 50px;
    margin-left: -25px;
    border-radius: 50% 50% 50% 50%;
    text-align: center;
    font-size: 1.4em;
    line-height: 50px;
    color: #fff;
    background-color: #FFF;
    border: 1px solid #d4d4d4;
}
.timeline > li > .timeline-panel {
    float: left;
    position: relative;
    width: 41.3%;
    padding: 2%;
    border: 1px solid #d4d4d4;
    border-radius: 2px;
    -webkit-box-shadow: 0 1px 6px rgba(0,0,0,0.175);
    box-shadow: 0 1px 6px rgba(0,0,0,0.175);
}
.timeline > li:hover .timeline-panel{
    border: 1px solid #3498db;
}
.timeline > li:hover .timeline-panel:before{
    border-top: 15px solid transparent;
    border-right: 0 solid #3498db;
    border-bottom: 15px solid transparent;
    border-left: 15px solid #3498db;
}
.timeline > li:hover .timeline-badge{
    border: 1px solid #3498db;
}
.timeline > li:hover .fa-check:before{
    color: #3498db;
}
.timeline > .timeline-inverted:hover .timeline-panel:before{
    border-top: 15px solid transparent;
    border-right: 15px solid #3498db;
    border-bottom: 15px solid transparent;
    border-left: 0 solid #3498db;
}
.timeline > li > .timeline-panel:before{
    content: " ";
    display: inline-block;
    position: absolute;
    top: 26px;
    right: -15px;
    border-top: 15px solid transparent;
    border-right: 0 solid #d4d4d4;
    border-bottom: 15px solid transparent;
    border-left: 15px solid #d4d4d4;
}
.timeline > li > .timeline-panel:after {
    content: " ";
    display: inline-block;
    position: absolute;
    top: 27px;
    right: -14px;
    border-top: 14px solid transparent;
    border-right: 0 solid #fff;
    border-bottom: 14px solid transparent;
    border-left: 14px solid #fff;
}
#timeLine_message{
	overflow: hidden;
	position: relative;
}

