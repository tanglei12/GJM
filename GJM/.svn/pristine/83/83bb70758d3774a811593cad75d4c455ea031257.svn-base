package com.gjp.model;

import java.util.Date;
import java.util.List;

/**
 * 客户日志
 * @author shenhx
 * 2017-06-12
 *
 */
public class UserCustomerLog {

	// 主键编码
	private Integer cl_id;
	// 客户编码
	private String cc_code;
	// 日志记录类型
	private Integer cl_type;
	// 日志内容
	private String cl_content;
	// 日志记录人
	private Integer cl_author;
	// 日志来源1-人工；2-系统
	private Integer cl_source;
	// 日志记录时间
	private Date cl_createTime;
	
	/******** 扩展 ********/
	// 房源编号
	private String hi_code;
	// 合同编号
	private String contractObject_code;
	// 日志记录类型中文
	private String cl_typeZH;
	// 管家名称
	private String em_name;
	// 附件
	private List<UserCustomerLogAttachment> userCustomerLogAttachmentList;
	
	public Integer getCl_id() {
		return cl_id;
	}
	public void setCl_id(Integer cl_id) {
		this.cl_id = cl_id;
	}
	public String getCc_code() {
		return cc_code;
	}
	public void setCc_code(String cc_code) {
		this.cc_code = cc_code;
	}
	public String getHi_code() {
		return hi_code;
	}
	public void setHi_code(String hi_code) {
		this.hi_code = hi_code;
	}
	public String getContractObject_code() {
		return contractObject_code;
	}
	public void setContractObject_code(String contractObject_code) {
		this.contractObject_code = contractObject_code;
	}
	public Integer getCl_type() {
		return cl_type;
	}
	public void setCl_type(Integer cl_type) {
		this.cl_type = cl_type;
	}
	public String getCl_content() {
		return cl_content;
	}
	public void setCl_content(String cl_content) {
		this.cl_content = cl_content;
	}
	public Integer getCl_author() {
		return cl_author;
	}
	public void setCl_author(Integer cl_author) {
		this.cl_author = cl_author;
	}
	public Integer getCl_source() {
		return cl_source;
	}
	public void setCl_source(Integer cl_source) {
		this.cl_source = cl_source;
	}
	public Date getCl_createTime() {
		return cl_createTime;
	}
	public void setCl_createTime(Date cl_createTime) {
		this.cl_createTime = cl_createTime;
	}
	public String getCl_typeZH() {
		return cl_typeZH;
	}
	public void setCl_typeZH(String cl_typeZH) {
		this.cl_typeZH = cl_typeZH;
	}
	public List<UserCustomerLogAttachment> getUserCustomerLogAttachmentList() {
		return userCustomerLogAttachmentList;
	}
	public void setUserCustomerLogAttachmentList(List<UserCustomerLogAttachment> userCustomerLogAttachmentList) {
		this.userCustomerLogAttachmentList = userCustomerLogAttachmentList;
	}
	public String getEm_name() {
		return em_name;
	}
	public void setEm_name(String em_name) {
		this.em_name = em_name;
	}
	
}
