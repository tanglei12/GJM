}
#search-box #pagaText { background: #fff; border: 1px solid #ddd; height: 22px; width: 22px; }
#search-box #pageNo {font-size: 14px;}
/**#### 弹窗样式 -START-------**/
#model-box { -moz-user-select: none; -webkit-user-select: none; }
