fieldset.fd-hint > legend { font-size: 15px; font-weight: bold; color: #F39C12; }
fieldset.fd-hint > ul.fd-hint-content { position: relative; width: 100%; }
fieldset.fd-hint > ul.fd-hint-content > li { display: block; font-size: 14px; line-height: 24px; position: relative; }
fieldset.fd-hint > ul.fd-hint-content > li:BEFORE { content: ""; display: block; position: absolute; top: 50%; left: -10px; margin-top: -2px; height: 4px; width: 4px; border-radius: 50%; background: #3498DB; -moz-box-shadow: 0 0 4px #3498DB; -webkit-box-shadow: 0 0 4px #3498DB; box-shadow: 0 0 4px #3498DB; }
fieldset.fd-hint > ul.fd-hint-content > li:HOVER { background-color: #e0effa; }
/**#### 列表温馨提示 -END-------**/

/**#### 公共选择标签 -START-------**/
.common-radio > input[type='radio'],
.common-checkbox > input[type='checkbox'],
.common-checkbox > input[type='radio'],
.common-borderbox > input[type='checkbox'],
.common-borderbox > input[type='radio'] { display: none !important; }
.common-radio { position: relative; border: 1px solid #ccc; color: #888; padding: 8px 16px; font-size: 14px; display: block; float: left; margin-right: 12px; cursor: pointer; -moz-border-radius: 4px; -webkit-border-radius: 4px; border-radius: 4px; -moz-user-select: none; -webkit-user-select: none; }
.common-radio-checked { border-color: #1ABC9C; color: #1ABC9C; }
.common-radio-disabled { background: #ddd; }
.common-radio-checked:BEFORE { content: ""; position: absolute; right: 1px; bottom: 1px; width: 14px; height: 14px; background: url("/resources/image/true.png") no-repeat; }
.common-borderbox { position: relative; border: 1px solid #ccc; color: #888; padding: 0 16px; line-height: 34px; font-size: 14px; display: block; float: left; margin-right: 12px; cursor: pointer; -moz-border-radius: 4px; -webkit-border-radius: 4px; border-radius: 4px; -moz-user-select: none; -webkit-user-select: none; }
.common-borderbox-checked { border-color: #1ABC9C; color: #1ABC9C; }
.common-borderbox-disabled { background: #ccc; cursor: not-allowed; }
.common-borderbox-checked:BEFORE { content: ""; position: absolute; right: 1px; bottom: 1px; width: 14px; height: 14px; background: url("/resources/image/true.png") no-repeat; z-index: 1; }
.common-borderbox-add { position: relative; border: 2px solid #1ABC9C; color: #ffffff; padding: 0 16px; font-size: 14px; display: block; float: left; height: 36px; margin-right: 12px; cursor: pointer; border-radius: 4px; background: #1ABC9C; }
.common-checkbox { position: relative; min-width: 20px; text-indent: 24px; display: block; float: left; cursor: pointer; -moz-user-select: none; -webkit-user-select: none; }
.common-checkbox:BEFORE {
    content: "";
    display: block;
    position: absolute;
    top: 50%;
    left: 0;
    margin-top: -10px;
    width: 20px;
    height: 20px;
    background: url("/resources/image/checkbox.png") no-repeat 0 -90px;
}
.common-checkbox-checked:BEFORE {
    /*content: "\f05d";*/
    background-position: -60px -30px !important;
}
