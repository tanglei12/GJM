*{padding: 0;margin: 0;-webkit-box-sizing: border-box;-moz-box-sizing: border-box;box-sizing: border-box;}
:after,:before {-webkit-box-sizing: border-box;-moz-box-sizing: border-box;box-sizing: border-box;}
a{ color:#666; text-decoration:none;}
a:hover{ color:#1ABC9C;}
body {font: 12px/150% '微软雅黑',Arial, Verdana, "\5b8b\4f53";min-width: 980px;color: #666;}
.content-center{
	padding: 15px;
}

.house-left{
	width: 646px;
	border: 1px solid #ECECEC;
	height: 500px;
	float: left;
}
.house-left .house-left-title{
	height: 40px;
	line-height: 40px;
	border-bottom: 1px solid #ECECEC;
	position: relative;
	text-indent: 20px;
	font-size: 14px;
}
.house-left .house-left-title:AFTER {
    content: "";
    display: block;
    width: 6px;
    height: 16px;
    position: absolute;
    background: #3e97c9;
    top: 12px;
    left: 8px;
    border-radius: 2px;
    visibility: visible;
}
.house-left table{
	width: 100%;
	border-collapse:collapse;
}
.house-left table thead{
	background-color: #f0f3f6;
	width: 100%;
	font-size: 13px;
}
.house-left table thead td{
	height: 35px;
	line-height: 35px;
	text-indent: 8px;
}
.house-left table tbody tr{
	height: 35px;
	line-height: 35px;
	border-bottom: 1px solid #ECECEC;
}
.house-left table tbody tr td{
	text-indent: 8px;
	font-size: 12px;
}
.house-left table tbody tr td .success-icon{
	display: block;
	width: 54px;
	height: 25px;
	line-height: 25px;
	background-color: #1ABC9C;
	color: #FFF;
	margin-left: 5px;
	border-radius: 3px;
}
.house-left table tbody tr td .error-icon{
	display: block;
	width: 54px;
	height: 25px;
	line-height: 25px;
	background-color: #E74C3C;
	color: #FFF;
	margin-left: 5px;
	border-radius: 3px;
}

.house-right{
	float: left;
	width: 274px;
	min-height: 500px;
	border: 1px solid #ECECEC;
	border-left: 0;
	position: relative;
}
.house-right .house-right-title{
	height: 40px;
	line-height: 40px;
	border-bottom: 1px solid #ECECEC;
	position: relative;
	text-indent: 20px;
	font-size: 14px;
}
.house-right .house-right-title:AFTER {
    content: "";
    display: block;
    width: 6px;
    height: 16px;
    position: absolute;
    background: #3e97c9;
    top: 12px;
    left: 8px;
    border-radius: 2px;
    visibility: visible;
}
.house-right dl{
	width: 100%;
	height: 35px;
	line-height: 35px;
	margin: 20px 0;
}
.house-right dl dt{
	float: left;
	font-size: 12px;
	width: 20%;
	text-align: center;
}
.house-right dl dd{
	float: left;
	width: 80%;
}
.house-right .newDl{
}
.house-right .newDl dt{
	float: left;
	font-size: 12px;
	width: 20%;
	height:37px;
	line-height: 37px;
	text-align: center;
}
.house-right .newDl dd{
	float: left;
	width: 80%;
}
.house-right .newDl dd input {
    width: 180px;
    height: 32px;
    line-height: 32px;
    border: 0;
    border: 1px solid #CCC;
    text-indent: 7px;
    color: #666;
    font-size: 15px;
    background-color: #fdf7f7;
    border-radius: 3px;
    margin-top: 4px;
}
.house-right button{
	width: 80px;
	height: 30px;
	background-color: #3498DB;
	border: 0;
	color: #FFF;
	border-radius: 4px;
	position: absolute;
	right: 20px;
	bottom: 20px;
	cursor: pointer;
	outline:none;
}

/*自定义checkbox*/
.house-left table .checkbox-min {position: relative;display: block;width: 34px;}
.checkbox-min .input_check {display: none;}
.checkbox-min .input_check+span {display: inline-block;border: 1px solid #a9a9a9;width: 16px;height: 16px;position: absolute;top: 50%;left: 50%;margin-top: -8px;margin-left: -8px; border-radius: 2px; background-color: #FFF;}
.checkbox-min .input_check:checked+span{border: 1px solid #18bc9c}
.checkbox-min .input_check:checked+span:after {content: "";position: absolute;left: 0px; bottom: 5px; width: 15px; height: 9px;border: 2px solid #18bc9c;border-top-color: transparent;border-right-color: transparent;-ms-transform: rotate(-60deg); -moz-transform: rotate(-60deg); -webkit-transform: rotate(-60deg); transform: rotate(-45deg);}  
.checkbox-min i{font-style: normal;margin-left: 20px;}
