@charset "utf-8";
/* CSS Document */

*{font-size:9pt;border:0;margin:0;padding:0;font-style:normal;}
body{font-family:'微软雅黑'; margin:0 auto;min-width:980px;}
ul{display:block;margin:0;padding:0;list-style:none;}
li{display:block;margin:0;padding:0;list-style: none;}
img{border:0;}
dl,dt,dd,span{margin:0;padding:0;display:block;}
a,a:focus{text-decoration:none;color:#000;outline:none;blr:expression(this.onFocus=this.blur());}
a:hover{color:#00a4ac;text-decoration:none;}
table{border-collapse:collapse;border-spacing: 0;}
cite{font-style:normal;}
h2{font-weight:normal;}

.place{height:40px; background:url(../image/righttop.gif) repeat-x;}
.place span{line-height:40px; font-weight:bold;float:left; margin-left:12px;}
.placeul li{float:left; line-height:40px; padding-left:7px; padding-right:12px; background:url(../image/rlist.gif) no-repeat right;}
.placeul li:last-child{background:none;}

/*组织架构*/
.frameworks{ width:98%; padding:20px 1%; overflow:hidden; height:auto;}
.frameworks td {font-size: 15px; line-height: 15px;}
.frameworks i{ width:3px; height:20px; background:url(../image/frame.png) no-repeat; display:block; margin-left:auto; margin-right:auto; margin-top: -1px;}
.frameworks a{display:block;background: url(../image/toolbg.gif); border: solid 1px #d3dbde; padding:7px; border-radius:5px;}

#orgLayer{width: 750px; height: 450px;}
#layerTitle{width: 740px; height: 35px; padding: 10px 0px 0px 10px; font-size: 16px; font-weight: bold; color: white; background-color: #3e97c9;}
.orgNav{width: 300px; height: 40px; margin: 22px 0px 12px 0px;}
.orgNav li{width: 100px; height: 32px; padding: 8px 0px 0px 0px; display: block; float: left; background-color: #ccc; text-align: center; font-size: 16px; font-weight: bold; color: #666; cursor: pointer;}
#moduleTittle{width: 120px; height: 22px; margin: 0px 0px 0px 5px;}
#moduleTittle div{width: 4px; height: 19px; background-color: #3b95c8; float: left; margin: 2px 3px 0px 0px;}
#moduleTittle span{ font-size: 16px; font-weight: bold; color: black;}
.orgFun{width: 360px; height: 36px; margin: 0px 0px 0px 0px; display: block; float: none;}
.orgCnt{width: 700px; height: 290px; margin: 10px 0px 0px 0px;}
.deptList{width: 550px; margin: 30px 0px 0px 60px;}
.deptList li{height: 30px; font-size: 14px;}
/*.deptList input{height: 18px; border: 1px solid #ccc;}*/
.deptList span{display:inline;}

/*form*/
.formbody{padding:10px 18px;}
.formtitle{border-bottom:solid 1px #d0dee5; line-height:35px; position:relative; height:35px; margin-bottom:28px;}
.formtitle span{font-weight:bold;font-size:14px; border-bottom:solid 3px #66c9f3;float:left; position:absolute; z-index:100; bottom:-1px; padding:0 3px; height:30px; line-height:30px;}
.forminfo{padding-left:23px;}
.forminfo li{margin-bottom:13px; clear:both;}
.forminfo li label{width:86px;line-height:34px; display:block; float:left;}
/*.forminfo li i{color:#7f7f7f; padding-left:20px; font-style:normal;}*/
.forminfo li cite{display:block; padding-top:10px;}
.dfinput{width:245px; height:32px; line-height:32px; border-top:solid 1px #a7b5bc; border-left:solid 1px #a7b5bc; border-right:solid 1px #ced9df; border-bottom:solid 1px #ced9df; background:url(../images/inputbg.gif) repeat-x; text-indent:10px;}
.textinput{border-top:solid 1px #a7b5bc; border-left:solid 1px #a7b5bc; border-right:solid 1px #ced9df; border-bottom:solid 1px #ced9df; background:url(../image/inputbg.gif) repeat-x; padding:10px; width:504px; height:135px; line-height:20px; overflow:hidden;}
.btn{width:80px;height:35px; background:url(../image/btnbg.png) no-repeat; font-size:14px;font-weight:bold;color:#fff; cursor:pointer;}

