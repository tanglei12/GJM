/** 显示窗口*/
.model-mark{
	opacity: 1.03;
    display: none;
    background-color: rgba(0, 0, 0, 0.4);
    position: fixed;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    z-index: 1;
}
.model-content{
	background-color: white;
    width: 1200px;
    padding-bottom: 17px;
    background: #fff;
    border-radius: 5px;
    text-align: center;
    position: fixed;
    left: 28%;
    top: 26%;
    margin-left: -256px;
    margin-top: -200px;
    overflow: hidden;
    display: none;
    z-index: 2;
}
.model-content>i{
	position: absolute;
	top: 0;
	right: 0;
	width: 35px;
	height: 35px;
	background: url("../../image/del2.png") no-repeat;
	cursor: pointer;	
}
.model-content #descTitle {
	margin: 0 auto;
    font-size: 22px;
    line-height: 32px;
    text-align: center;
    outline: none;
    width: 380px;
    border-bottom: 1px solid #ddd;
}

.model-content .content-box {
	padding: 20px 0;
    width: 380px;
    margin: 0 auto;
	padding-bottom: 0;
}
.model-content .content-box .content-top {margin: 0 2px;position: relative;}
.model-content .content-box .content-top:AFTER{
	content: ".";
	display: block;
	clear: both;
	height: 0;
	visibility: hidden;
}
.model-content .content-box .content-top>input{
	display: block;
    float: left;
	height: 34px;
    line-height: 34px;
    width: 309px;
    text-align: center;
    border: 1px solid #1abc9c;
    background: #fff;
    color: #333;
    font-size: 14px;
    outline: none;
/*     -moz-border-radius: 4px; */
/*     -webkit-border-radius: 4px; */
    border-top-left-radius: 3px;
    border-bottom-left-radius: 3px;
}
.model-content .content-box .content-top>button{
	background-color: #1abc9c;
    color: white;
    border: none;
    float: left;
    box-shadow: none;
    font-size: 15px;
    font-weight: 500;
    padding: 0 18px;
    height: 34px;
    line-height: 34px;
    cursor: pointer;
    outline: none;
    border-top-right-radius: 3px;
    border-bottom-right-radius: 3px;
}
.model-content .content-box .content-tisp {
	height: 24px;
    line-height: 20px;
	text-align: left;
    margin: 0 2px;
}

.model-content .content-box .content-list {	position: relative;}
.model-content .content-box .content-list>span>label{
	display: block;
	width: 87px;
	overflow:hidden;
    white-space:nowrap;
    cursor: pointer;
    text-overflow:ellipsis;
    float: left;
}
.model-content .content-box .content-list>span:HOVER>i{display: block;}
.model-content .content-box #list1>span{
    border: 1px solid #3498DB;
    background: #fff;
    color: #185e8b;
}
.model-content .content-box .purpose{
    float: left;
    width: 31px;
    text-align: center;
    color: #FFFFFF;
    background: #18bc9c;
    height: 30px;
    line-height: 30px;
    font-size: 11px;
    border-radius: 0 3px 3px 0;
}
.model-content .content-box .purpose.close{
    background: #dddddd;
    font-size: 11px;
}
.list1-span-style{
    background: #3498DB !important;
    color: #fff !important;
}
#subServiceTitle{
	text-align: center;
}
.model-tisp{
	position: relative;
	width: 380px;
	margin: 0 auto;
}
.model-tisp>h3{
	text-align: left;
	line-height: 30px;
}
.model-tisp>ul{list-style: inherit !important;}
.model-tisp>ul li {
	list-style: inherit !important;
	text-align: left;
	line-height: 15px;
	color: #666;
}

.error{color: #E74C3C !important;}
.success{color: #27AE60 !important;}

.model-content .model-list{
	position: relative;
	width: 300px;
	padding-top: 6px;
}
.model-content .content-list>span{
	position: relative;
	display: block;
	float: left;
    height: 32px;
    line-height: 32px;
    font-size: 14px;
    border-radius: 0px;
    padding: 0;
    margin: 0 3px 6px 3px;
    text-align: center;
    border: 1px solid #1abc9c;
    background: #1abc9c;
    color: #fff;
    cursor: pointer;
    white-space:nowrap;
    text-overflow:ellipsis;
    border-radius: 3px;
}
.model-content .content-list>span>i{
	position: absolute;
    top: -9px;
    right: -9px;
    width: 20px;
    height: 20px;
    -moz-border-radius: 50%;
    -webkit-border-radius: 50%;
    border-radius: 50%;
    display: none;
    cursor: pointer;
    background: url("../../image/del21.png") no-repeat center;
}

.model-content>dl>dd>span{
	display: block;
	background: #1abc9c;
    color: #fff;
	height: 32px;
    line-height: 32px;
    min-width: 80px;
    width: 80px;
    text-align: center;
    font-size: 14px;
    outline: none;
    -moz-border-radius: 4px;
    -webkit-border-radius: 4px;
    border-radius: 4px;
}

.model-content #model-save{
    background-color: #AEDEF4;
    color: white;
    border: none;
    box-shadow: none;
    font-size: 15px;
    font-weight: 500;
    border-radius: 5px;
    padding: 8px 26px;
    margin: 26px 5px 0 5px;
    cursor: pointer;
    outline: none;
}
