/*自定义checkbox*/
.checkbox-min {position: relative;display: block; float: left;}
.checkbox-min .input_check {display: none;}
.checkbox-min .input_check+span {display: inline-block;border: 1px solid #a9a9a9;width: 16px;height: 16px;position: absolute;top: 50%;left: 10px;margin-top: -8px;margin-left: -8px; border-radius: 2px; background-color: #FFF;}
.checkbox-min .input_check:checked+span{border: 1px solid #18bc9c}
.checkbox-min .input_check:checked+span:after {content: "";position: absolute;left: 0px; bottom: 5px; width: 15px; height: 9px;border: 2px solid #18bc9c;border-top-color: transparent;border-right-color: transparent;-ms-transform: rotate(-60deg); -moz-transform: rotate(-60deg); -webkit-transform: rotate(-60deg); transform: rotate(-45deg);}
.checkbox-min div{margin: 0 0 0 25px; padding: 0; overflow: hidden; display: block;}