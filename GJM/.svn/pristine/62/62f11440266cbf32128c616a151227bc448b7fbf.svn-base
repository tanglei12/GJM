@CHARSET "UTF-8";
/*****/
* {
    font-size: 9pt;
    border: 0;
    margin: 0;
    padding: 0;
}
/*****/
body {
	font-family: "微软雅黑";
	font-size: 14px;
	background: #f5f5f5;
	color: #333;
}

dl,dt,dd,ul,li{list-style: none;}
dl:AFTER,dd:AFTER,ul:AFTER,li:AFTER,#main-box-content:AFTER,.sub-main:AFTER,.model-box:AFTER,.content-list:AFTER{
	content: "";
	display: block;
	clear: both;
	height: 0;
	visibility: hidden;
}
/*****/
a,a:focus{text-decoration:none;outline:none;blr:expression(this.onFocus=this.blur());}
a:hover{color:#00a4ac;text-decoration:none;}
.place{height:40px; background:url(../../image/righttop.gif) repeat-x;}
.place span{line-height:40px; font-weight:bold;float:left; margin-left:12px;}
.placeul li{float:left; line-height:40px; padding-left:7px; padding-right:12px; background:url(../../image/rlist.gif) no-repeat right;}
.placeul li:last-child{background:none;}
/*****/

